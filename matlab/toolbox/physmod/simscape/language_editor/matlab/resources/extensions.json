{
    "jsModules": "./generatedModuleDef.json",
    "mw.desktop.editor.languageSupport": [
        {
            "languageName": "Simscape",
            "mimeType": "text/x-simscape",
            "languagePriority": "PRODUCT",
            "defaultSupportedFileTypes": [
                "ssc"
            ],
            "commentStyle": {
                "lineComment": "%"
            },
            "jsModule": "simscapeLanguageRegistry"
        }
    ],
    "mw.fileTypes": {
        "open": {
            "ssc": [
                {
                    "callback": "editor_open_action/openAction",
                    "callbackType": "jsModule"
                },
                {
                    "callback": "edit",
                    "callbackType": "matlab"
                }
            ]
        },
        "icons": {
            "ssc": "simscapeFT",
            "sscp": "simscapeFT"
        }
    }
}
