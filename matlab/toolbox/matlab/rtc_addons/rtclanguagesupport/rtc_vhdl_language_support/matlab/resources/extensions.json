{
    "jsModules": "./generatedModuleDef.json",
    "mw.desktop.editor.languageSupport": [{
        "languageName": "VHDL",
        "mimeType": "text/x-vhdl",
        "languagePriority": "COMMON",
        "defaultSupportedFileTypes": ["vhd", "vhdl"],
        "commentStyle": {
            "lineComment": "--"
        },
        "jsModule": "vhdlLanguageRegistry"
    }],
    "mw.fileTypes": {
        "groups": {
            "vhdlCodeFiles": ["vhd", "vhdl"]
        },
        "open": {
            "groups.vhdlCodeFiles": [{
                "callback": "editor_open_action/openAction",
                "callbackType": "jsModule"
            }, {
                "callback": "edit",
                "callbackType": "matlab"
            }]
        }
    }
}