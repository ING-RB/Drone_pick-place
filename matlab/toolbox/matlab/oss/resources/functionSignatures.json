{
    "computer":
{
    "inputs":
    [
        {"name":"'arch'", "kind":"flag", "type":["choices={'arch'}"]}
    ],
    "outputs":
    [
        {"name":"architecture", "kind":"ordered"},
        {"name":"maxsize", "kind":"ordered"},
        {"name":"endian", "kind":"ordered"}
    ]

},
"dos":
{
    "inputs":
    [
        {"name":"command","kind":"required","type":"char"},
        {"name":"'-echo'","kind":"flag","type":"choices={'-echo'}"}
    ],
    "outputs":
    [
        {"name":"status","type":["double","scalar"]},
        {"name":"data","type":"char"}
    ]
},
"unix":
{
    "inputs":
    [
        {"name":"command","kind":"required","type":"char"},
        {"name":"'-echo'","kind":"flag","type":"choices={'-echo'}"}
    ],
    "outputs":
    [
        {"name":"status","type":["double","scalar"]},
        {"name":"data","type":"char"}
    ]
},
"system":
{
    "inputs":
    [
        {"name":"command","kind":"required","type":"char"},
        {"name":"'-echo'","kind":"flag","type":"choices={'-echo'}"}
    ],
    "outputs":
    [
        {"name":"status","type":["double","scalar"]},
        {"name":"data","type":"char"}
    ]
},
"memory":
{
    "outputs":
    [
        {"name":"USERVIEW"},
        {"name":"SYSTEMVIEW"}
    ]
},
"getenv":
{
    "inputs":
    [
        {"name":"name", "kind":"required", "type":[["string"], ["choices=getenv().keys"]]}
    ],
    "outputs":
    [
        {"name":"value"}
    ]
},
"isenv":
{
    "inputs":
    [
        {"name":"name", "kind":"required", "type":[["string"], ["choices=getenv().keys"]]}
    ],
    "outputs":
    [
        {"name":"value", "type": "logical"}
    ]
},
"unsetenv":
{
    "inputs":
    [
        {"name":"name", "kind":"required", "type":[["string"], ["choices=getenv().keys"]]}
    ]
},
"setenv":
{
    "inputs":
    [
        {"name":"name",  "kind":"required", "type":[["string"], ["choices=getenv().keys"]]},
        {"name":"value", "kind":"required", "type":"string"}
    ]
},
"loadenv":
{
    "inputs":
    [
        {"name":"filename", "kind": "required", "type": [["file=*.env"], ["string"], ["char"]]},
        {"name":"Encoding", "kind":"namevalue", "type":[["char","choices={'auto','system'}"], ["char","choices=matlab.io.internal.shared.EncodingInput.encodingSuggestions()"]], "default":"'UTF-8'"},
        {"name":"OverwriteEnvironmentVariable","kind":"namevalue", "type":["logical","scalar"],"default":"true"},
        {"name":"FileType","kind":"namevalue", "type":"choices={'auto', 'env'}","default":"'auto'"},
        {"name":"ExpandVariables","kind":"namevalue", "type":["logical","scalar"],"default":"true"}
    ],
    "outputs":
    [
        { "name": "out", "type": "dictionary"}
    ]
}
}
