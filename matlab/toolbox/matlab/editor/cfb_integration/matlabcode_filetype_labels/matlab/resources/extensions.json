            "m-livescript": "%editor_integration:actions:newLiveScriptLabel%",
            "m-function": "%editor_integration:actions:newFunctionLabel%",
            "m-livefunction": "%editor_integration:actions:newLiveFunctionLabel%",
            "m-class": "%editor_integration:actions:newClassLabel%",
            "mlx-script": "%editor_integration:actions:newLiveScriptLabel%",
            "mlx-function": "%editor_integration:actions:newLiveFunctionLabel%"
        },
        "plugins": {
            "groups.matlabCodeFiles": [{
                "callback": "matlabcode_filetype_labels/MatlabCodeFileLabelPlugin",
                "callbackType": "jsModule"
            }]
        }
