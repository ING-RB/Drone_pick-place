    {"name":"Border", "kind":"namevalue", "type":[["choices={'tight','loose'}"], ["scalar"]]},
    {"name":"Colormap", "kind":"namevalue", "type":["numeric", "size=:,3", ">=0", "<=1"]},
    {"name":"DisplayRange", "kind":"namevalue", "type":["numeric", "size=1,2"]},
