    {"name":"Interpolation", "kind":"namevalue", "type":[["choices={'nearest','bilinear'}"], ["scalar"]]},
    {"name":"Parent", "kind":"namevalue", "type":"matlab.graphics.axis.Axes"},
    {"name":"Reduce", "kind":"namevalue", "type":"logical"},
    {"name":"XData", "kind":"namevalue", "type":"numeric"},
    {"name":"YData", "kind":"namevalue", "type":"numeric"}
  ],
  "outputs":
  [
    {"name":"himage", "type":"matlab.graphics.primitive.Image"}
  ]
},

