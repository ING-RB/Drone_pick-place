"imshow":
{
  "inputs":
  [
    {"name":"I", "kind":"required", "type":[["logical"], ["numeric"], ["folder"], ["matlabpathfile=*.bmp,*.cur,*.fts,*.fits,*.gif,*.hdf,*.ico,*.j2c,*.j2k,*.jp2,*.jpf,*.jpx,*.jpg,*.jpeg,*.pbm,*.pcx,*.pgm,*.png,*.pnm,*.ppm,*.ras,*.tif,*.tiff,*.xwd"]]},
