"rgb2hsv":
{
  "inputs":
  [
    { "mutuallyExclusiveGroup":
             [
                 [
                     {"name":"rgbmap", "kind":"required", "type":[["single","size=:,3"],["double","size=:,3"]]}
                 ],
                 [
                     {"name":"RGB", "kind":"required", "type":[["single","size=:,:,3"],["double","size=:,:,3"],["uint8","size=:,:,3"],["uint16","size=:,:,3"]]}
                 ]
             ]
            }
  ],
  "outputs":
  [
    { "mutuallyExclusiveGroup":
             [
                 [
                     {"name":"hsvmap", "type":"inferredFrom=rgbmap"}
                 ],
                 [
                     {"name":"H", "type":"inferredFrom=RGB"}
                 ]
             ]
            }
  ]
},   

