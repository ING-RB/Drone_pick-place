{
    "readlines":
    {
        "inputs":
        [
            {"name":"filename", "kind":"required", "type":[["file"], "char"]},
            {"name":"WhitespaceRule", "kind":"namevalue", "type":["char","choices={'trim','trimleading','trimtrailing','preserve'}"],"default":"'trim'"},
            {"name":"Encoding", "kind":"namevalue", "type":[["char"],["string","scalar"]], "default":"''"},
            {"name":"EmptyLineRule","kind":"namevalue", "type":"choices={'skip', 'read', 'error'}"},
            {"name":"Whitespace","kind":"namevalue", "type":"char"},
            {"name":"LineEnding","kind":"namevalue", "type":[["char"],["cellstr"]]}
        ],
        "outputs":
        [
            {"name":"lines", "type":"string"}
        ]
    }
}
