{
    "serialportlist": {
        "inputs": [
            {
                "name": "ports",
                "kind": "ordered",
                "type": "choices={'all','available'}"
            }
        ]
    },
    "serialport": {
        "inputs": [
            {
                "name": "Port",
                "kind": "required",
                "type": "choices = serialportlist()"
            },
            {
                "name": "BaudRate",
                "kind": "required",
                "type": [ "numeric", ">=0" ]
            },
            {
                "name": "ByteOrder",
                "kind": "namevalue",
                "type": "choices={'little-endian', 'big-endian'}"
            },
            {
                "name": "DataBits",
                "kind": "namevalue",
                "type": "choices={5, 6, 7, 8}"
            },
            {
                "name": "FlowControl",
                "kind": "namevalue",
                "type": "choices={'none','hardware','software'}"
            },
            {
                "name": "Parity",
                "kind": "namevalue",
                "type": "choices={'none','odd','even'}"
            },
            {
                "name": "StopBits",
                "kind": "namevalue",
                "type": "choices={1, 1.5, 2}"
            },
            {
                "name": "Tag",
                "kind": "namevalue",
                "type": [
                    [ "scalar", "string" ],
                    [ "char" ]
                ]
            },
            {
                "name": "Timeout",
                "kind": "namevalue",
                "type": "double"
            }
        ],
        "outputs": [
            {
                "name": "obj",
                "type": "internal.Serialport"
            }
        ]
    },
    "internal.Serialport.delete": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            }
        ]
    },
    "internal.Serialport.read": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "count",
                "kind": "required",
                "type": [ "integer", "scalar", ">0" ]
            },
            {
                "name": "precision",
                "kind": "required",
                "type": "choices= matlabshared.transportlib.internal.client.GenericClient.PrecisionOptions"
            }
        ],
        "outputs": [
            {
                "name": "data",
                "type": [
                    [ "double" ],
                    [ "char" ],
                    [ "string" ]
                ]
            }
        ]
    },
    "internal.Serialport.readline": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            }
        ],
        "outputs": [
            {
                "name": "data",
                "type": "string"
            }
        ]
    },
    "internal.Serialport.readbinblock": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "precision",
                "kind": "ordered",
                "type": "choices= matlabshared.transportlib.internal.client.GenericClient.PrecisionOptions"
            }
        ],
        "outputs": [
            {
                "name": "data",
                "type": [
                    [ "double" ],
                    [ "char" ],
                    [ "string" ]
                ]
            }
        ]
    },
    "internal.Serialport.write": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "data",
                "kind": "required",
                "type": [
                    [ "numeric" ],
                    [ "char" ],
                    [" string" ]
                ]
            },
            {
                "name": "precision",
                "kind": "required",
                "type": "choices= matlabshared.transportlib.internal.client.GenericClient.PrecisionOptions"
            }
        ]
    },
    "internal.Serialport.writeline": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "data",
                "kind": "required",
                "type": [ "char", "string" ]
            }
        ]
    },
    "internal.Serialport.writebinblock": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "data",
                "kind": "required",
                "type": [
                    [ "numeric" ],
                    [ "char" ],
                    [ "string" ]
                ]
            },
            {
                "name": "precision",
                "kind": "required",
                "type": "choices= matlabshared.transportlib.internal.client.GenericClient.PrecisionOptions"
            }
        ]
    },
    "internal.Serialport.writeread": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "command",
                "kind": "required",
                "type": [ "char", "string" ]
            }
        ],
        "outputs": [
            {
                "name": "response",
                "type": [ "char", "string" ]
            }
        ]
    },
    "internal.Serialport.flush": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "buffer",
                "kind": "ordered",
                "type": "choices={'input','output'}"
            }
        ]
    },
    "internal.Serialport.setDTR": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "flag",
                "kind": "required",
                "type": [ "logical", "scalar" ]
            }
        ]
    },
    "internal.Serialport.setRTS": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "flag",
                "kind": "required",
                "type": [ "logical", "scalar" ]
            }
        ]
    },
    "internal.Serialport.getpinstatus": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            }
        ],
        "outputs": [
            {
                "name": "status",
                "type": "struct"
            }
        ]
    },
    "internal.Serialport.configureCallback": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "mode",
                "kind": "required",
                "type": "choices={'off', 'byte', 'terminator'}"
            },
            {
                "name": "count",
                "kind": "positional",
                "type": [ "integer", "scalar", ">0" ]
            },
            {
                "name": "callbackfcn",
                "kind": "positional",
                "type": [ "function_handle", "scalar" ]
            }
        ]
    },
    "internal.Serialport.configureTerminator": {
        "inputs": [
            {
                "name": "obj",
                "kind": "required",
                "type": "internal.Serialport"
            },
            {
                "name": "terminator",
                "kind": "required",
                "type": [
                    [ "integer", "scalar", ">=0", "<=255" ],
                    [ "choices={'LF', 'CR', 'CR/LF'}" ]
                ]
            },
            {
                "name": "writeTerminator",
                "kind": "optional",
                "type": [
                    [ "integer", "scalar", ">=0", "<=255" ],
                    [ "choices={'LF', 'CR', 'CR/LF'}" ]
                ]
            }
        ]
    },
    "serialportfind":{
        "inputs": [
            {
                "name": "Tag",
                "kind": "namevalue",
                "type": [
                    [ "scalar", "string" ],
                    [ "char" ]
                ]
            },
            {
                "name": "Port",
                "kind": "namevalue",
                "type": "serialportlist()"
            },
            {
                "name": "BaudRate",
                "kind": "namevalue",
                "type": [ "numeric", ">=0" ]
            },
            {
                "name": "ByteOrder",
                "kind": "namevalue",
                "type": "choices = matlabshared.transportlib.internal.TabCompletionUtility.ByteOrder"
            },
            {
                "name": "Parity",
                "kind": "namevalue",
                "type": "choices = matlabshared.transportlib.internal.TabCompletionUtility.Parity"
            },
            {
                "name": "DataBits",
                "kind": "namevalue",
                "type": "choices = matlabshared.transportlib.internal.TabCompletionUtility.DataBits"
            },
            {
                "name": "StopBits",
                "kind": "namevalue",
                "type": "choices = matlabshared.transportlib.internal.TabCompletionUtility.StopBits"
            },
            {
                "name": "FlowControl",
                "kind": "namevalue",
                "type": "choices = matlabshared.transportlib.internal.TabCompletionUtility.FlowControl"
            },
            {
                "name": "Timeout",
                "kind": "namevalue",
                "type": [ "numeric", ">=0" ]
            },
            {
                "name": "NumBytesAvailable",
                "kind": "namevalue",
                "type": [ "numeric", "scalar" ]
            },
            {
                "name": "NumBytesWritten",
                "kind": "namevalue",
                "type": [ "numeric", "scalar" ]
            },
            {
                "name": "Terminator",
                "kind": "namevalue",
                "type":  "choices = matlabshared.transportlib.internal.TabCompletionUtility.Terminator"
            },
            {
                "name": "BytesAvailableFcnMode",
                "kind": "namevalue",
                "type": "choices = matlabshared.transportlib.internal.TabCompletionUtility.BytesAvailableFcnMode"
            },
            {
                "name": "BytesAvailableFcnCount",
                "kind": "namevalue",
                "type": [ "numeric", ">=0" ]
            },
            {
                "name": "BytesAvailableFcn",
                "kind": "namevalue",
                "type": [ "function_handle", "scalar" ]
            },
            {
                "name": "ErrorOccurredFcn",
                "kind": "namevalue",
                "type": [ "function_handle", "scalar" ]
            },
            {
                "name": "UserData",
                "kind": "namevalue",
                "type": []
            }
        ],
        "outputs": [
            {
                "name": "obj",
                "type": "internal.Serialport"
            }
        ]
    }
}