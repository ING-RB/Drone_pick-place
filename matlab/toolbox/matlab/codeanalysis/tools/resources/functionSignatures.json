{
    "codeIssues":
    {
        "inputs":
        [
            {"name":"names", "kind":"required", "type":[["file=*.m,*.mlx,*.mlapp"],["folder"],["cellstr"]]},
            {"name":"IncludeSubfolders", "kind":"namevalue", "type":["logical", "scalar"]},
            {"name":"CodeAnalyzerConfiguration", "kind":"namevalue", "type":[["file=*.txt"], ["choices=[\"active\",\"factory\",]"]]}
        ]
    },
    "codeIssues.fix":
    {
        "inputs":
        [
            {"mutuallyExclusiveGroup":
                [
                    [
                        {"name":"obj", "kind":"required", "type":["codeIssues"]},
                        {"name":"checkID", "kind":"required", "type":["string", "choices=matlab.codeanalyzer.internal.findFixableCheckIds(obj)"]},
                        {"name":"filename", "kind":"positional", "type":["string", "choices=matlab.codeanalyzer.internal.findFixableFiles(obj, checkID)"]}
                    ],
                    [
                        {"name":"obj", "kind":"required", "type":["codeIssues"]},
                        {"name":"issuesTable", "kind":"required", "type":["table"]}
                    ]
                ]
            }
        ]
    },
    "codeIssues.export":
    {
        "inputs":
        [
            {"name":"obj", "kind":"required", "type":["codeIssues"]},
            {"name":"filename", "kind":"positional", "type":["string"]},
            {"name":"FileFormat", "kind":"namevalue", "type":"choices={'auto', 'json', 'sarif', 'sonarqube'}"},
            {"name":"SourceRoot", "kind":"namevalue", "type":["string"]}
        ]
    },
    "matlab.codeanalysis.validateConfiguration":
    {
        "inputs":
        [
            {"name":"name", "kind":"required", "type":[["file=*.json"],["folder"]]}
        ]
    }
}
