"graph.simplify":
{
    "inputs":
    [
        {"mutuallyExclusiveGroup":
            [
                [
                    {"name":"G", "kind":"required", "type":["graph"]},
                    {"name":"selfLoops", "kind":"ordered", "type":["char","choices={'omitselfloops', 'keepselfloops'}"]}
                ],
                [
                    {"name":"G", "kind":"required", "type":["graph"]},
                    {"name":"pickmethod", "kind":"required", "type":["char","choices={'first', 'last'}"]},
                    {"name":"selfLoops", "kind":"ordered", "type":["char","choices={'omitselfloops', 'keepselfloops'}"]}
                ],
                [
                    {"name":"G", "kind":"required", "type":["graph"]},
                    {"name":"pickmethod", "kind":"required", "type":["char","choices={'min', 'max'}"]},
                    {"name":"selfLoops", "kind":"ordered", "type":["char","choices={'omitselfloops', 'keepselfloops'}"]},
                    {"name":"PickVariable", "kind":"namevalue", "type":[["char","choices=setdiff(G.Edges.Properties.VariableNames, 'EndNodes')"],["cellstr"],["numeric","scalar"]]}
                ],
                [
                    {"name":"G", "kind":"required", "type":["graph"]},
                    {"name":"reducemethod", "kind":"required", "type":["char","choices={'sum', 'mean'}"]},
                    {"name":"selfLoops", "kind":"ordered", "type":["char","choices={'omitselfloops', 'keepselfloops'}"]},
                    {"name":"AggregationVariables", "kind":"namevalue", "type":[["char","choices=setdiff(G.Edges.Properties.VariableNames, 'EndNodes')"],["cellstr"],["numeric","vector"],["logical","vector"],["function_handle"]]}
                ]
            ]
        }
    ],
    "outputs":
    [
        {"name":"gsimple", "type":["graph"]},
        {"name":"edgeind", "type":["double","vector"]},
        {"name":"edgecount", "type":["double","vector"]}
    ]
},
