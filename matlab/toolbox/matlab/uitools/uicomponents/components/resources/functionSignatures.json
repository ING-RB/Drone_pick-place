{  
    "uialert":
    {
        "inputs":
        [
            {"name":"figure", "kind":"required", "type":"matlab.ui.Figure"},
            {"name":"message", "kind":"required", "type":[["char"], ["cellstr"]]},
            {"name":"title", "kind":"required", "type":"char"},
            {"name":"CloseFcn", "kind":"namevalue", "type":[["function_handle"], ["char"], ["cellstr"]]},
            {"name":"Icon", "kind":"namevalue", "type":[["choices={'error', 'warning', 'info', 'success', 'none'}"], ["file"], ["numeric"]]},
            {"name":"Modal", "kind":"namevalue", "type":["logical", "scalar"]},
            {"name":"Interpreter", "kind":"namevalue", "type":"choices={'none','tex', 'latex','html'}"}
        ]
    },

    "uiconfirm":
    {
        "inputs":
        [
            {"name":"f", "kind":"required", "type":"matlab.ui.Figure"},
            {"name":"message", "kind":"required", "type":[["char"], ["cellstr"]]},
            {"name":"title", "kind":"required", "type":"char"},
            {"name":"Icon", "kind":"namevalue", "type":[["choices={'error', 'warning', 'info', 'success', 'question', 'none'}"], ["file"], ["numeric"]]},
            {"name":"CloseFcn", "kind":"namevalue", "type":[["function_handle"], ["char"], ["cellstr"]]},
            {"name":"Options", "kind":"namevalue", "type":"cellstr"},
            {"name":"DefaultOption", "kind":"namevalue", "type":[["char"], ["integer"]]},          
            {"name":"CancelOption", "kind":"namevalue", "type":[["char"], ["integer"]]},
            {"name":"Interpreter", "kind":"namevalue", "type":"choices={'none','tex', 'latex','html'}"}
        ]
    },

    "uifigure":
    {
        "inputs":
        [
            {"name":"opts", "kind":"properties", "type":"matlab.ui.Figure"}
        ]
    },

    "uiprogressdlg":
    {
        "inputs":
        [
            {"name":"f", "kind":"required", "type":"matlab.ui.Figure"},
            {"name":"Message", "kind":"namevalue", "type":[["char"], ["string"], ["cellstr"]]},
            {"name":"Title", "kind":"namevalue", "type":[["char"], ["string", "scalar"]]},
            {"name":"Value", "kind":"namevalue", "type":["numeric", ">=0", "<=1"]},
            {"name":"Icon", "kind":"namevalue", "type":[["choices={'error', 'warning', 'info', 'success', 'question', 'none'}"], ["file"], ["numeric"]]},
            {"name":"Indeterminate", "kind":"namevalue", "type":"choices={'on','off'}"},
            {"name":"ShowPercentage", "kind":"namevalue", "type":"choices={'on','off'}"},
            {"name":"Cancelable", "kind":"namevalue", "type":"choices={'on','off'}"},          
            {"name":"CancelText", "kind":"namevalue", "type":[["char"], ["string", "scalar"]]},
            {"name":"Interpreter", "kind":"namevalue", "type":"choices={'none','tex', 'latex','html'}"}
        ]
    },
    "matlab.ui.componentcontainer.ComponentContainer":
    {
        "inputs":
        [
            {"name":"parent", "kind":"ordered", "type":[["matlab.ui.Figure"], ["matlab.ui.container.Panel"], ["matlab.ui.container.Tab"], ["matlab.ui.container.ButtonGroup"], [ "matlab.ui.container.GridLayout"]]},
            {"name":"opts", "kind":"properties", "type":"matlab.ui.componentcontainer.ComponentContainer"}
        ],
        "outputs":
        [
            {"name":"h", "type":"matlab.ui.componentcontainer.ComponentContainer"}
        ]
    }
}
