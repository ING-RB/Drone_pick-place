			[
				[
					{"name":"method", "kind":"required", "type":["char", "choices={'auto'}"]}
				],
				[
					{"name":"method", "kind":"required", "type":["char", "choices={'circle'}"]},
					{"name":"Center", "kind":"namevalue", "type":[["numeric","scalar"],["cellstr","scalar"],["char","choices=getNodeNames(H)"]]}
				],
				[
					{"name":"method", "kind":"required", "type":["char", "choices={'force'}"]},
					{"name":"Iterations", "kind":"namevalue", "type":[["numeric","scalar"]]},
					{"name":"WeightEffect", "kind":"namevalue", "type":["char", "choices={'direct', 'inverse', 'none'}"]},
					{"name":"UseGravity", "kind":"namevalue", "type":["char", "choices={'on', 'off'}"]},
					{"name":"XStart", "kind":"namevalue", "type":[["numeric","vector"]]},
					{"name":"YStart", "kind":"namevalue", "type":[["numeric","vector"]]}
				],
				[
					{"name":"method", "kind":"required", "type":["char", "choices={'force3'}"]},
					{"name":"Iterations", "kind":"namevalue", "type":[["numeric","scalar"]]},
