"validatecolor":
{
    "inputs":
    [
        {"name":"colors", "kind":"required", "type":
            [
                ["cellstr", "vector"],
                ["string", "vector"],
                ["numeric", "ncols=3"]
            ]
        },
        {"name":"sz", "kind":"flag", "type":["char", "choices={'one','multiple'}"]}
     ],
    "outputs":
    [
        {"name":"RGB", "type":["double", "ncols=3"]}
    ]
},
"rgb2hex":
{
    "inputs":
    [
        {"name":"RGB", "kind":"required", "type":
