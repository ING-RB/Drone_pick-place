{
"exportgraphics":
{
    "inputs":
    [
    	{"name":"target", "kind":"required", "type":[["matlab.ui.Figure"],["matlab.graphics.axis.AbstractAxes"],["matlab.graphics.chart.Chart"]]},
        {"name":"fileSpec", "kind":"required", "type":"char"},
        {"name":"Colorspace", "kind":"namevalue", "type":"choices={'rgb', 'cmyk', 'gray'}", "default":"'rgb'"},
        {"name":"Append", "kind":"namevalue", "type":"choices={true, false}", "default":false},
        {"name":"ContentType", "kind":"namevalue", "type":"choices={'auto', 'vector', 'image'}", "default":"'auto'"},
        {"name":"Resolution", "kind":"namevalue", "type":["numeric","scalar","choices={150, 300, 600}"], "default":150},
    	{"name":"BackgroundColor", "kind":"namevalue", "type":
           [
              ["char", "choices={'black', 'blue', 'cyan', 'green', 'magenta', 'red', 'yellow', 'white','none','current'}"],
              ["numeric", "2d", "ncols=3", ">=0", "<=1"],
              ["char", "@matlab.graphics.internal.isColorSpec"]
           ], "default":"white"
        },
        {"name":"PreserveAspectRatio", "kind":"namevalue", "type":"choices={'auto', 'on', 'off'}", "default":"'auto'"},
        {"name":"Units", "kind":"namevalue", "type":"choices={'auto', 'inches', 'centimeters', 'points', 'pixels'}", "default":"'auto'"},
        {"name":"Width", "kind":"namevalue", "type":
            [
                ["char", "choices={'auto'}"],
                ["numeric", "size=1,1", ">0", "<Inf"]
            ], "default":"'auto'"
        },
        {"name":"Height", "kind":"namevalue", "type":
            [
                ["char", "choices={'auto'}"],
                ["numeric", "size=1,1", ">0", "<Inf"]
            ], "default":"'auto'"
        },
        {"name":"Padding", "kind":"namevalue", "type":
            [
                ["char", "choices={'tight', 'figure'}"],
                ["numeric", "size=1,1", ">=0", "<Inf"]
            ], "default":"'tight'"
        }
    ]
}, 

