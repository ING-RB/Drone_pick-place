{

    "controllerMPPI":
        {
            "inputs":
                [
                    {
                        "name": "refPath",
                        "kind": "required",
                        "type": [
                            [
                                "numeric",
                                "2d",
                                "@(x)(width(x)==2 || width(x)==3) && height(x)>2"
                            ],
                            [
                                "navPath",
                                "@(x)class(x.StateSpace)=='stateSpaceSE2'"
                            ]
                        ]
                    },
                    {
                        "name": "VehicleModel",
                        "kind": "namevalue",
                        "type": [
                            [
                                "ackermannKinematics"
                            ],
                            [
                                "bicycleKinematics"
                            ],
                            [
                                "unicycleKinematics"
                            ],
                            [
                                "differentialDriveKinematics"
                            ]
                        ]
                    },
                    {
                        "name": "Map",
                        "kind": "namevalue",
                        "type": [
                            [
                                "binaryOccupancyMap"
                            ],
                            [
                                "occupancyMap"
                            ],
                            [
                                "signedDistanceMap"
                            ]
                        ]
                    },
                    {
                        "name": "Options",
                        "kind": "namevalue",
                        "type": [
                            [
                                "struct",
                                "@(S)all(isfield(S,{'CostFcn','CostWeights','Parameters'}))"
                            ]
                        ]
                    },
                    {
                        "name": "NumTrajectory",
                        "kind": "namevalue",
                        "type": [
                            "numeric",
                            "integer",
                            "scalar",
                            "positive"
                        ]
                    },
                    {
                        "name": "StandardDeviation",
                        "kind": "namevalue",
                        "type": [
                            "numeric",
                            "row",
                            "numel=2",
                            ">=0",
                            "real"
                        ]
                    },
                    {
                        "name": "TrajectorySelectionBias",
                        "kind": "namevalue",
                        "type": [
                            "numeric",
                            "scalar",
                            "positive",
                            "real"
                        ]
                    },
                    {
                        "name": "SampleTime",
                        "kind": "namevalue",
                        "type": [
                            "numeric",
                            "scalar",
                            "positive",
                            "real"
                        ]
                    },
                    {
                        "name": "LookaheadTime",
                        "kind": "namevalue",
                        "type": [
                            "numeric",
                            "scalar",
                            "positive",
                            "real"
                        ]
                    },
                    {
                        "name": "GoalTolerance",
                        "kind": "namevalue",
                        "type": [
                            "numeric",
                            "row",
                            "numel=3",
                            "positive",
                            "real"
                        ]
                    }
                ]
        },
    "controllerMPPI.step":
        {
            "inputs":
                [
                    {
                        "name": "mppi",
                        "kind": "required",
                        "type": "controllerMPPI"
                    },
                    {
                        "name": "currentState",
                        "kind": "required",
                        "type": [
                            "numeric",
                            "row",
                            "@(x)numel(x)==3 || numel(x)==4",
                            "real"
                        ]
                    },
                    {
                        "name": "currentControl",
                        "kind": "required",
                        "type": [
                            "numeric",
                            "row",
                            "numel=2",
                            "real"
                        ]
                    }
                ]
        },

