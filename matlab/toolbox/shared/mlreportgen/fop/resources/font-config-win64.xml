<?xml version="1.0" encoding="UTF-8" ?>
<!-- DOCTYPE font-config SYSTEM "font-config.dtd" -->
<font-config>
  <acc-folder/>
  <font-folder path="@@USERWINFONTS@@"/>
  <font-folder path="C:\Windows\Fonts"/>
  <font-folder path="$matlabroot\toolbox\shared\mlreportgen\dom\resources\fonts">
    <font-alias  file="NotoSansCJKjp-Regular.otf">
      <alias family-name="Noto Sans CJK JP" weight="normal"/>
      <alias family-name="Noto Sans CJK KR" weight="normal"/>
      <alias family-name="Noto Sans CJK SC" weight="normal"/>
      <alias family-name="Noto Sans CJK TC" weight="normal"/>
    </font-alias>
  </font-folder>
  
</font-config>

 
