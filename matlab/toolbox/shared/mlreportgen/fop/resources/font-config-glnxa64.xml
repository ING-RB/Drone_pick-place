<?xml version="1.0" encoding="UTF-8" ?>
<!-- DOCTYPE font-config SYSTEM "font-config.dtd" -->
<font-config>
  <font-folder path="$matlabroot/sys/ahformatter/fonts">
    <font-entry file="ZapfDingbats.afm"
          glyph-list="ZapfDingbats-glyphname.txt"/>
  </font-folder>
  <font-folder path="$matlabroot/toolbox/shared/mlreportgen/dom/resources/fonts">
    <font-alias  file="NotoSansCJKjp-Regular.otf">
      <alias family-name="Noto Sans CJK JP" weight="normal"/>
      <alias family-name="Noto Sans CJK KR" weight="normal"/>
      <alias family-name="Noto Sans CJK SC" weight="normal"/>
      <alias family-name="Noto Sans CJK TC" weight="normal"/>
    </font-alias>  
  </font-folder>
  <font-folder path="$user.home/.fonts" recursive="true"/>
  <font-folder path="/usr/local/fonts" recursive="true"/>
  <font-folder path="/usr/local/share/fonts" recursive="true"/>
  <font-folder path="/usr/share/fonts" recursive="true"/>
  <font-folder path="/usr/X11R6/lib/X11/fonts" recursive="true"/>
</font-config>

