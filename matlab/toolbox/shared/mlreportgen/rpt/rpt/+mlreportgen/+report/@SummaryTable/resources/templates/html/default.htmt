<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>SummaryTable Template</title>
    <style>/*<![CDATA[*/ 

    /* Default styles for SummaryTable table titles. */
    p.SummaryTableTableTitle {
        font-size: 12pt;
        margin-top: 12pt;
        margin-bottom: 7.3pt;
        font-weight:bold;
        color: black;
        white-space: pre;
    }

    /* Default styles for SummaryTable tables. */
    table.SummaryTableTable {
        border-collapse:collapse;
    }

    /* Default styles for SummaryTable table content. */
    table.SummaryTableTable td {
        border-style:solid;
        border-width:1px;
        border-collapse:collapse;
        font-size: 16px;
        font-style: normal;
        font-variant: normal;
        font-weight: normal;
        line-height: normal;
        text-align: start;
        white-space: normal;
    }

    /* Default styles for SummaryTable table headers. */
    table.SummaryTableTable th {
        border-style:solid;
        border-width:1px;
        background-color:#D3D3D3;
        font-weight: bold;
        font-size: 16px;
        font-style: normal;
        font-variant: normal;
        line-height: normal;
        text-align: left;
        white-space: normal;
    }
    
        //]]>
    </style>
</head>
<body>
  <dplibrary>
        <dptemplate name="SummaryTable">
            <!-- filled with BaseTable object -->
            <hole id="TableContent">TABLE_CONTENT</hole>
        </dptemplate>

        <!-- hierarchical numbered template for the BaseTable title -->
        <dptemplate name="SummaryTableHierNumberedTitle">
            <p class="SummaryTableTableTitle" style="counter-increment:table"
                ><hole id="NumberPrefix">NUMBER_PREFIX</hole
                ><autonumber stream-name="h1" format="n"></autonumber
                ><span>.</span
                ><autonumber stream-name="table" format="n"></autonumber
                ><hole id="NumberSuffix">NUMBER_SUFFIX</hole
                ><hole id="Content">TITLE_CONTENT</hole
            ></p>
        </dptemplate>
            
        <!-- numbered template for the BaseTable title -->
        <dptemplate name="SummaryTableNumberedTitle">
            <p class="SummaryTableTableTitle" style="counter-increment:table"
                ><hole id="NumberPrefix">NUMBER_PREFIX</hole
                ><autonumber stream-name="table" format="n"></autonumber
                ><hole id="NumberSuffix">NUMBER_SUFFIX</hole
                ><hole id="Content">TITLE_CONTENT</hole
            ></p>
        </dptemplate>
  </dplibrary>
</body>
</html>
