      <Property name="StringValue" type="ustring"/>
    </member>
    
    <!-- SDI.SIGNALMETRIC: Single measurement value for a signal -->
    <member  name="SignalMetric" type="Class">
      <Property isIndexed="true" name="Signal" type="sdi.Signal" />
      <Property isIndexed="true" indexedProperties="Signal" name="Name" type="string"/>
      <Property name="Value" type="real64"/>
    </member>
    
    <!-- SDI.PAUSEDMODEL: Used for importing logged data during simulation pause. When a model
