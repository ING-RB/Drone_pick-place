    <!-- SDI.CLASSDEFINITION: Details about a given Data Type for Signals -->
    <member  name="ClassDefinition" type="Class">
      <Property name="Name" type="string"/>
      <Property name="Classification" type="string"/>
      <Property isIndexed="true" name="CheckSum" type="int32"/>
      <Property name="InstanceID" type="int64"/>
    </member>

    <!-- SDI.ENUMERATIONVALUE: Name-Value pair for a given enumeration -->
    <member  name="EnumerationValue" type="Class">
      <Property name="Value" type="int32"/>
      <Property name="Label" type="string"/>
      <Property name="ClassDefinition" isIndexed="true" type="sdi.ClassDefinition" />
    </member>

    <!-- SDI.STRINGVALUE: Name-Value pair for a given string (value for string data type) -->
    <member  name="StringValueMapping" type="Class">
      <Property name="Signal" isIndexed="true" type="sdi.Signal" />
