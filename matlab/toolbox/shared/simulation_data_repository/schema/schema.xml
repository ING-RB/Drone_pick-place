      <Property isIndexed="true" name="Handle" type="real64"/>
      <Property isIndexed="true" name="SetID" type="sdi.DataSet"/>
    </member>

    <!-- SDI.SIGNALOBSERVERS: Non-default signal observer settings for a given signal -->
    <member  name="SignalObservers" type="Class">
      <Property isIndexed="true" name="SignalID" type="sdi.Signal"/>
      <Property name="ObserverType" type="string"/>
      <Property name="UpdateRate" type="int32"/>
      <Property isIndexed="true" name="ClientUUID" type="int64"/>
    </member>

    <!-- SDI.OBSERVERPARAMS: Non-default observer parameters for a given signal observer.
         Observer parameters are structures where each field can be an array of strings,
         doubles, integers, or Booleans. So the default SDI signal adaptors would be
         represented in the database as follows:
         
         SignalObserverID ParamName    FieldName    FieldType FieldArrayIndex StringVal DoubleVal  IntVal LogicalVal
         ================ =========    =========    ========= =============== ========= ========== ====== ==========
         164              Adaptors     "Name"      "string"  1               "min_max" 
         164              Adaptors     "Name"      "string"  2               "quant" 
         164              Adaptors     "Name"      "string"  3               "rwv2pix"
         
         And line settings might be represented as follows:
         
         SignalObserverID ParamName    FieldName    FieldType FieldArrayIndex StringVal DoubleVal  IntVal LogicalVal
         ================ =========    =========    ========= =============== ========= ========== ====== ==========
         164              LineSettings "Color"      "double"  1                         0.7
         164              LineSettings "Color"      "double"  2                         0.1
         164              LineSettings "Color"      "double"  3                         0.4
         164              LineSettings "LineStyle"  "string"  1              ":"
    -->
    <member  name="ObserverParams" type="Class">
      <Property name="SignalObserverID" type="sdi.SignalObservers"/>
      <Property name="ParamName" type="string"/>
      <Property name="FieldName" type="string"/>
      <Property name="FieldType" type="string"/>
      <Property name="FieldArrayIndex" type="int32"/>
      <Property name="StringVal" type="string"/>
      <Property name="DoubleVal" type="real64"/>
      <Property name="IntVal" type="int32"/>
      <Property name="LogicalVal" type="int32"/>
    </member>
    
