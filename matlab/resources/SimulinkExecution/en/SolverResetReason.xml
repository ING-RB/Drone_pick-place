<rsccat version="1.0"
        locale="en_US"
        product="SimulinkExecution"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../schema/.xsd">

  <message>
    <entry key="None" translate="false">
      No reset
    </entry>

    <entry key="Unspecified" translate="false">
      Unknown
    </entry>

    <entry key="BlockContinuousStateChange" translate="false">
      block continuous state change
    </entry>

    <entry key="BlockModeChange" translate="false">
      block mode change
    </entry>

    <entry key="BlockEnableState" translate="false">
      block state enabled
    </entry>

    <entry key="BlockDisableState" translate="false">
      block state disabled
    </entry>

    <entry key="BlockUnspecifiedStateChange" translate="false">
      block unknown state change
    </entry>

    <entry key="SolverParameterChange" translate="false">
      solver or simulation parameter change
    </entry>

    <entry key="ModelParameterChange" translate="false">
      model parameter change
    </entry>

    <entry key="BlockParameterChange" translate="false">
      block parameter change
    </entry>

    <entry key="Discontinuity" translate="false">
      zero crossing
    </entry>

    <entry key="DiscreteDerivative" translate="false">
      discrete derivative
    </entry>

    <entry key="ZOHDerivative" translate="false">
      fix in minor derivative
    </entry>

    <entry key="SystemModeChange" translate="false">
      modelwide mode change
    </entry>

    <entry key="InitialReset" translate="false">
      initial reset
    </entry>

    <entry key="InternalReset" translate="false">
      internal solver reset
    </entry>

    <entry key="DiscontinuityUnrelatedToDE" translate="false">
      zero crossing that does not impact ode
    </entry>
    
    <entry key="DESolverChanged" translate="false">
      auto solver change
    </entry> 

    <entry key="SFunctionUnspecifiedReset" translate="false">
      S-Function requested solver reset using ssSetSolverNeedsReset for unspecified reason
    </entry>

    <entry key="SecondaryZeroCrossing" translate="false">
      Block state change due to zero crossing
    </entry>

    <entry key="SecondaryZOHInput" translate="false">
      Block state change due to ZOH input signal
    </entry>

    <entry key="SecondaryDiscreteInput" translate="false">
      Block state change due to discrete input signal
    </entry>
    
  </message>

</rsccat>

