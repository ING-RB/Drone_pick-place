      <attribute id="iar.attr.nested"              name="__nested"/>
      <attribute id="iar.attr.non_banked"          name="__non_banked"/>
      <attribute id="iar.attr.noreturn"            name="__noreturn"/>
      <attribute id="iar.attr.no_pic"              name="__no_pic"/>
      <attribute id="iar.attr.no_save"             name="__no_save"/>
      <attribute id="iar.attr.overlay_near_func"   name="__overlay_near_func"/>
      <attribute id="iar.attr.pdata_reentrant"     name="__pdata_reentrant"/>
      <attribute id="iar.attr.ramfunc"             name="__ramfunc"/>
      <attribute id="iar.attr.raw"                 name="__raw"/>
