      <attribute id="iar.attr.idata"           name="__idata"/>
      <attribute id="iar.attr.io"              name="__io"/>
      <attribute id="iar.attr.ixdata"          name="__ixdata"/>
      <attribute id="iar.attr.near"            name="__near"/>
