<?xml version="1.0"?>
<!-- Copyright 2015-2016 The MathWorks, Inc.

     All definitions for the following IAR targets:
       - icc8051  (CPU family: 8051)
       - iccavr   (CPU family: Atmel AVR)
       - iccm16c  (CPU family: Renesas M16C/IX-3X, 5X-6X and R8C)
       - iccr32c  (CPU family: Renesas R32C/100)
       - icc78k,
         icc78k0r (CPU family: Renesas 78K0/78K0S and 78K0R)
       - iccrh850 (CPU family: Renesas RH850)
       - iccrl78  (CPU family: Renesas RL78)
       - iccrx    (CPU family: Renesas RX)
       - iccsh    (CPU family: Renesas RH)
       - iccv850  (CPU family: Renesas V850)
       - icc430   (CPU family: Texas Instruments MSP430)
       - iccstm8  (CPU family: STMicroelectronics STM8)

     This file is intended for inclusion in a <compilers> node.
-->
<compiler name="iar" lang="c"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="extensions.xsd">
  <!-- Modern IAR: IAR version >= 2.0 -->

  <clients>
    <client name="matlab"/>
  </clients>

  <options>
    <option name="--c99"/>
    <option name="--no_c99_generic"/> <!-- Some C99 implementations use __generic() in <tgmath.h>; clashes with IAR's __generic. -->
    <option name="--allow_long_float"/>
    <option name="--allow_non_int_bitfield"/>
    <option name="--nonstd_anonymous_unions"/>
    <option name="--enable_static_assert"/>
    <option name="--extended_variadic_macros"/>
  </options>

  <asms>
    <asm name="asm"/>
  </asms>

  <attributes kind="data">
    <attribute name="@" sig="@"/>
  </attributes>

  <library>
    <!--
         All IAR attribute definitions
    -->

    <!-- IAR memory, type, and object attributes -->
    <attributes kind="data">
      <attribute id="iar.attr.bdata"           name="__bdata"/>
      <attribute id="iar.attr.bit"             name="__bit"/>
      <attribute id="iar.attr.bitvar"          name="__bitvar"/>
      <attribute id="iar.attr.brel"            name="__brel"/>
      <attribute id="iar.attr.brel23"          name="__brel23"/>
      <attribute id="iar.attr.code"            name="__code"/>
      <attribute id="iar.attr.data"            name="__data"/>
      <attribute id="iar.attr.data13"          name="__data13"/>
      <attribute id="iar.attr.data16"          name="__data16"/>
      <attribute id="iar.attr.data20"          name="__data20"/>
      <attribute id="iar.attr.data24"          name="__data24"/>
      <attribute id="iar.attr.data28"          name="__data28"/>
      <attribute id="iar.attr.data32"          name="__data32"/>
      <attribute id="iar.attr.eeprom"          name="__eeprom"/>
      <attribute id="iar.attr.ext_io"          name="__ext_io"/>
      <attribute id="iar.attr.far"             name="__far"/>
      <attribute id="iar.attr.far22"           name="__far22"/>
      <attribute id="iar.attr.far22_code"      name="__far22_code"/>
      <attribute id="iar.attr.far22_rom"       name="__far22_rom"/>
      <attribute id="iar.attr.farflash"        name="__farflash"/>
      <attribute id="iar.attr.far_code"        name="__far_code"/>
      <attribute id="iar.attr.far_rom"         name="__far_rom"/>
      <attribute id="iar.attr.flash"           name="__flash"/>
