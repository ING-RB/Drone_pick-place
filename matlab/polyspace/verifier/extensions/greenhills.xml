<compiler name="greenhills" default="powerpc"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="extensions.xsd">

  <clients>
    <client name="polyspace"/>
  </clients>

  <options>
    <option name="--lean_gnu_mode"/>
    <option name="--gnu_version" value="40300"/>
    <option name="--restrict"/>
    <option name="--enable_static_assert"/>
    <option name="--enable_static_assert_cpp"/>
    <option name="--allow_long_float"/>
    <option name="--preinclude" value="__polyspace_common.h"/>
    <option name="--preinclude" value="greenhills/tmw_builtins/intrinsic_greenhills.h"/>
    <option name="--va_list_is_void_star"/>
    <if feoption="--c\+\+">
      <option name="--c99_generic_in_cpp"/>
      <ifnot feoption="--c\+\+03">
        <ifnot feoption="--c\+\+11">
          <ifnot feoption="--c\+\+14">
            <ifnot feoption="--c\+\+17">
              <ifnot feoption="--c\+\+20">
                <option name="--c++03"/>
              </ifnot>
            </ifnot>
          </ifnot>
        </ifnot>
      </ifnot>
    </if>
    
    <if feoption="--c">
      <option name="--preserve_lvalues_with_same_type_casts"/>
      <option name="--set_flag" value="allow_anonymous_union_like_construct"/>
    </if>
    <option name="--nonstd_anonymous_unions"/>
  </options>

  <attributes>
    <attribute name="__noinline"/>
    <attribute name="__interrupt"/>
    <attribute name="__linkonce"/>
    <attribute name="__nearcall"/>
    <attribute name="__farcall"/>
    <attribute name="__packed" alias="gnu:packed" constraints="cL{specifier:post_tag_def,prefix:post_tag_def}|e|d|Wv|Wp|Wr|Wt"/>
    <attribute name="__callt"/>     <!-- Only available in RH850 but compiles in other targets -->

    <attribute name="__littleendian" token="tok_volatile"/>
    <attribute name="__bigendian" token="tok_volatile"/>
    <attribute name="__bytereversed" token="tok_volatile"/>

    <!-- Gnu-style attributes: __attribute__((...)) -->
    <!-- format: Greenhills compiler allows 3 arguments -->
    <attribute name="format" family="gnu" sig="(n,ci,ci?,ci)" attrkind="ak_format"/>
  </attributes>


  <!-- Macros common to all targets -->
  <defines>

    <!-- GHS Macros -->
    <define name="__ghs__" value="1"/>
    <define name="__ghs" value="1"/>
    <define name="__ghs_asm" value="1"/>
    <define name="__GHS_REVISION_DATE" value="&quot;Oct 19 2015&quot;"/>
    <define name="__GHS_REVISION_VALUE" value="1445230492"/>
    <define name="__GHS_VERSION_NUMBER" value="201516"/>
    <define name="__PROTOTYPES__" value="1"/>
    <define name="__ATTRIBUTES" value="1"/>
    <define name="__IeeeFloat__" value="1"/>
    <define name="__IeeeFloat" value="1"/>
    <define name="__ELF__" value="1"/>
    <define name="__ELF" value="1"/>

    <!-- Macros for the Language C or C++ -->
    <if feoption="--c\+\+">
      <define name="__LANGUAGE_CXX__"  value="1"/>
      <define name="__LANGUAGE_CXX"  value="1"/>
      <define name="__STANDARD_CXX"  value="1"/>
      <define name="__STANDARD_CXX_HEADERS"  value="1"/>
      <define name="__c_plusplus"  value="1"/>
      <define name="__GHS_NOCOMMONS__" value="1"/>
      <define name="__GHS_NOCOMMONS" value="1"/>
      <define name="__STDC__" value="0"/>
      <define name="__NAMESPACES" value="1"/>
      <!-- Macros for C++ Polyspace Libraries. -->
      <define name="_LIBCPP_COMPILER_GENERIC_POLYSPACE" value="1"/>
    </if>
    <if feoption="--c">
      <define name="__LANGUAGE_C__"  value="1"/>
      <define name="__LANGUAGE_C"  value="1"/>
    </if>

    <define name="__STDC_HOSTED__" value="1"/>

    <!-- GNU compatibilities -->
    <define name="__MW_GNU__" value="1"/>
    <define name="PST_GNU" value="1"/>
    <define name="_GNU_SOURCE" value="1"/>
    <define name="POLYSPACE_LIBC_NO_THROW" value="1"/>

    <!-- Polyspace varargs compatibilities -->
    <define name="__gh_va_list" value="va_list"/>

  </defines>

  <pragmas>
    <pragma name="asm" end="endasm"/>
  </pragmas>

  <asms>
    <asm name="__asm"/>
    <asm name="__asm__"/>
    <asm name="asm"/>
    <asm name="__asmleaf"/>
  </asms>

  <target name="powerpc" title="PowerPC">

    <options>
      <option name="--max_alignment"      value="8"/>
      <option name="--char_bit"           value="8"/>
      <option name="--sizeof_short"       value="2"/>
      <option name="--sizeof_int"         value="4"/>
      <option name="--sizeof_long"        value="4"/>
      <option name="--sizeof_long_long"   value="8"/>
      <option name="--sizeof_pointer"     value="4"/>
      <option name="--sizeof_float"       value="4"/>
      <option name="--sizeof_double"      value="8"/>
      <option name="--sizeof_long_double" value="8"/>

      <option name="--size_t_is_unsigned_int"/>
      <option name="--ptr_diff_is_int"/>
      <option name="--wchar_t_is_signed_long"/>

      <!-- Unsigned char is default in PowerPC
           Polyspace option -default-sign-of-chars used -->
      <ifnot feoption="--signed_chars">
        <option name="--unsigned_chars"/>
      </ifnot>

      <option name="--preinclude" value="greenhills/tmw_builtins/powerpc.h"/>
    </options>

    <!-- By default, Altivec is not supported -->
    <if option="-Altivec">
      <options>
        <option name="--altivec"/>
        <option name="--gnu_vector_types"/>
        <if feoption="--c">
          <option name="--compound_literals"/>
        </if>
      </options>

      <defines>
        <define name="__ALTIVEC__" value="1"/>
        <define name="__VEC__" value="10205"/>
        <define name="pixel" value="__pixel"/>
        <define name="vector" value="__vector"/>
      </defines>
    </if>

    <if option="-SPE">
      <defines>
        <define name="__SPE__" value="1"/>
      </defines>

      <types>
        <type name="__ev64_u16__"    type_kind="int"    decl_specs="vector8,short,unsigned"/>
        <type name="__ev64_s16__"    type_kind="int"    decl_specs="vector8,short"/>
        <type name="__ev64_u32__"    type_kind="int"    decl_specs="vector8,unsigned"/>
        <type name="__ev64_s32__"    type_kind="int"    decl_specs="vector8"/>
        <type name="__ev64_u64__"    type_kind="int"    decl_specs="vector8,long_long,unsigned"/>
        <type name="__ev64_s64__"    type_kind="int"    decl_specs="vector8,long_long"/>
        <type name="__ev64_opaque__" type_kind="opaque" decl_specs="vector8"/>
        <type name="__ev64_fs__"     type_kind="float"  decl_specs="vector8"/>
        <type name="__ev64_u5bit__"  type_kind="int"    decl_specs="vector8,unsigned"/>
        <type name="__ev64_s5bit__"  type_kind="int"    decl_specs="vector8"/>
      </types>

      <options>
        <option name="--gnu_vector_types"/>
        <option name="--allow_implicit_vector_conversions"/>
        <if feoption="--c">
          <option name="--compound_literals"/>
        </if>
      </options>
    </if>

    <defines>
      <define name="__ppc__" value="1"/>
      <define name="__ppc" value="1"/>
      <define name="__PowerPC__" value="1"/>
      <define name="__PowerPC" value="1"/>
      <define name="__powerpc__" value="1"/>
      <define name="__LSP_APU__" value="1"/>

      <!-- Unsigned char is default in PowerPC
           Polyspace option -default-sign-of-chars used -->
      <switch feoption="--signed_chars">
        <case>
          <define name="__Char_Is_Signed__" value="1"/>
          <define name="__SIGNED_CHARS__" value="1"/>
        </case>
        <default>
          <define name="__Char_Is_Unsigned__" value="1"/>
          <define name="__CHAR_UNSIGNED__" value="1"/>
        </default>
      </switch>

      <define name="__BigEndian__" value="1"/>
      <define name="__BigEndian" value="1"/>
      <define name="__BIG_ENDIAN__" value="1"/>
      <define name="__Field_Is_Unsigned__" value="1"/>
      <define name="__Ptr_Is_Unsigned__" value="1"/>
      <define name="__Enum_Field_Is_Signed__" value="1"/>
      <define name="__WChar_Is_Signed__" value="1"/>
      <define name="__WChar_Is_Long__" value="1"/>

      <define name="__CHAR_BIT" value="8"/>
      <define name="__SHRT_BIT" value="16"/>
      <define name="__INT_BIT" value="32"/>
      <define name="__LONG_BIT" value="32"/>
      <define name="__LLONG_BIT" value="64"/>
      <define name="__LL_BIT" value="64"/>
      <define name="__PTR_BIT" value="32"/>
      <define name="__FUNCPTR_BIT" value="32"/>
      <define name="__WCHAR_BIT" value="32"/>
      <define name="__REG_BIT" value="32"/>
      <define name="__FREG_BIT" value="64"/>
      <define name="__LDBL_BIT" value="64"/>
      <define name="__TIME_T_BIT" value="32"/>
      <define name="__MAX_BIT" value="64"/>

      <define name="__LDBL_SIZE" value="8"/>
      <define name="__Int_Is_32" value="1"/>
      <define name="__Long_Is_32" value="1"/>
      <define name="__LL_Is_64" value="1"/>
      <define name="__Ptr_Is_32" value="1"/>
      <define name="__Reg_Is_32" value="1"/>

      <define name="__DOUBLE_HL__" value="1"/>
      <define name="__DOUBLE_HL" value="1"/>
      <define name="__ghs_sda" value="1"/>
      <define name="__ghs_zda" value="1"/>
      <define name="__ghs_alignment" value="8"/>
      <define name="__ghs_max_pack_value" value="16"/>
    </defines>

  </target>

