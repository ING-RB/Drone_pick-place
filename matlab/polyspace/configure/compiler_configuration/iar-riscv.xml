<compiler_configuration>
  <compiler_names>
    <name>iccriscv</name>
  </compiler_names>
  <compiler_xml>
    <name>iar-ew</name>
    <opt numArgs="1">_noopt</opt>
    <target pattern="_" value="riscv"/>
  </compiler_xml>
  <compiler>iar-ew</compiler>
  <include_options>
    <opt>-I</opt>
  </include_options>
  <system_include_options>
    <opt>--system_include_dir</opt>
  </system_include_options>
  <preinclude_options>
    <opt>--preinclude</opt>
  </preinclude_options>
  <define_options>
    <opt>-D</opt>
  </define_options>
  <undefine_options>
    <opt>-U</opt>
  </undefine_options>
  <options_file_options>
    <opt>-f</opt>
    <opt>--f</opt>
  </options_file_options>
  <standard_options>
    <std version="c90">--c89</std>
    <std version="c17" c_default="1"></std>
    <std version="c++14" cpp_default="1"></std>
  </standard_options>
  <semantic_options>
    <opt>--c++</opt>
    <opt>--c89</opt>
    <opt>--char_is_signed</opt>
    <opt>--char_is_unsigned</opt>
    <opt numArgs="1">--core</opt>
    <opt>--debug</opt>
    <opt isPrefix="true" numArgs="2" isIgnored="true">--dependencies</opt>
    <opt numArgs="1" isIgnored="true">--deprecated_feature_warnings</opt>
    <opt numArgs="1" isIgnored="true">--diagnostics_tables</opt>
    <opt numArgs="1" isIgnored="true">--diag_error</opt>
    <opt numArgs="1" isIgnored="true">--diag_remark</opt>
    <opt numArgs="1" isIgnored="true">--diag_suppress</opt>
    <opt numArgs="1" isIgnored="true">--diag_warning</opt>
    <opt isIgnored="true">--discard_unused_publics</opt>
    <opt>--dlib</opt>
    <opt numArgs="1">--dlib_config</opt>
    <opt>--do_explicit_zero_opt_in_named_sections</opt>
    <opt>-e</opt>
    <opt>--enable_restrict</opt>
    <opt numArgs="1" isIgnored="true">--error_limit</opt>
    <opt>--guard_calls</opt>
    <opt isIgnored="true">--header_context</opt>
    <opt>--include_macro_defs</opt>
    <opt isPrefix="true" numArgs="2" isIgnored="true">-l</opt>
    <opt isIgnored="true">--macro_positions_in_diagnostics</opt>
    <opt numArgs="1">--max_cost_constexpr_call</opt>
    <opt>--mfc</opt>
    <opt isIgnored="true">--misrac_verbose</opt>
    <opt isIgnored="true">--misrac1998</opt>
    <opt isIgnored="true">--misrac2004</opt>
    <opt>--no_bom</opt>
    <opt>--no_call_frame_info</opt>
    <opt>--no_code_motion</opt>
    <opt>--no_cross_call</opt>
    <opt>--no_cross_jump</opt>
    <opt>--no_cse</opt>
    <opt>--no_default_fp_contract</opt>
    <opt>--no_exceptions</opt>
    <opt>--no_fragments</opt>
    <opt>--no_inline</opt>
    <opt>--no_path_in_file_macros</opt>
    <opt>--no_rtti</opt>
    <opt>--no_scheduling</opt>
    <opt>--no_size_constraints</opt>
    <opt>--no_static_destruction</opt>
    <opt>--no_system_include</opt>
    <opt>--no_tbaa</opt>
    <opt>--no_static_destruction</opt>
    <opt>--no_typedefs_in_diagnostics</opt>
    <opt>--no_uniform_attribute_syntax</opt>
    <opt>--no_unroll</opt>
    <opt>--no_warnings</opt>
    <opt>--no_wrap_diagnostics</opt>
    <opt isIgnored="true">--nonportable_path_warnings</opt>
    <opt isPrefix="true">-O</opt>
    <opt numArgs="1" isIgnored="true">-o</opt>
    <opt isIgnored="true">--only_stdout</opt>
    <opt numArgs="1" isIgnored="true">--output</opt>
    <opt numArgs="1">--pending_instantiations</opt>
    <opt numArgs="1" isIgnored="true">--predef_macros</opt>
    <opt numArgs="1">--public_equ</opt>
    <opt>--relaxed_fp</opt>
    <opt isIgnored="true">--remarks</opt>
    <opt>--require_prototypes</opt>
    <opt>-r</opt>
    <opt isIgnored="true">--silent</opt>
    <opt numArgs="1">--source_encoding</opt>
    <opt>--stack_protection</opt>
    <opt>--strict</opt>
    <opt>--strict_ansi</opt>
    <opt numArgs="1" isIgnored="true">--text_out</opt>
    <opt>--uniform_attribute_syntax</opt>
    <opt>--use_c++_inline</opt>
    <opt>--use_paths_as_written</opt>
    <opt>--use_unix_directory_separators</opt>
    <opt>--utf8_text_in</opt>
    <opt>--version</opt>
    <opt>--vla</opt>
    <opt isIgnored="true">--warnings_affect_exit_code</opt>
    <opt isIgnored="true">--warnings_are_errors</opt>
    <opt isIgnored="true">--warn_about_c_style_casts</opt>
  </semantic_options>
  <preprocess_options_list>
    <opt>--preprocess=n</opt>
    <opt>-</opt>
  </preprocess_options_list>
  <preprocess_dollar_macros>no</preprocess_dollar_macros>
  <strict_option_names>yes</strict_option_names>
  <forbidden_macros_list>
  </forbidden_macros_list>
  <compile_options_list>
  </compile_options_list>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>o</ext>
  </obj_extensions>
  <precompiled_header_extensions>
  </precompiled_header_extensions>
  <include_strategy>sniff</include_strategy>
</compiler_configuration>
