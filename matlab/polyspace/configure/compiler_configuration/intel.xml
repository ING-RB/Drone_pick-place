<compiler_configuration>
  <compiler_names>
    <name>icp?c</name>
  </compiler_names>
  <compiler>intel</compiler>
  <compiler_xml>
    <name>intel</name>
    <opt numArgs="1">_noopt</opt>
    <target pattern="_" value="x86_64"/>
  </compiler_xml>
  <include_options>
    <opt>-I</opt>
    <opt>-iquote</opt>
    <opt>-idirafter</opt>
  </include_options>
  <system_include_options>
    <opt>-isystem</opt>
  </system_include_options>
  <preinclude_options>
    <opt>-include</opt>
  </preinclude_options>
  <define_options>
    <opt removePhases="instrum">-D</opt>
  </define_options>
  <undefine_options>
    <opt>-U</opt>
  </undefine_options>
  <options_file_options>
    <opt>@</opt>
  </options_file_options>
  <standard_options>
    <std version="c90">/Qstd=c90</std>
    <std version="c99">/Qstd=c99</std>
    <std version="c11" c_default="1">/Qstd=c11</std>
    <std version="c17">/Qstd=c17</std>
    <std version="c++03">/Qstd=c++03</std>
    <std version="c++11">/Qstd=c++11</std>
    <std version="c++14" cpp_default="1">/Qstd=c++14</std>
    <std version="c++17">/Qstd=c++17</std>
    <std version="c++20">/Qstd=c++20</std>
  </standard_options>
  <semantic_options>
    <opt isPrefix="true">-A</opt>
    <opt>-alias-const</opt>
    <opt>-no-alias-const</opt>
    <opt>-align</opt>
    <opt>-noalign</opt>
    <opt>-ansi</opt>
    <opt>-ansi-alias</opt>
    <opt>-no-ansi-alias</opt>
    <opt>-ansi-alias-check</opt>
    <opt>-no-ansi-alias-check</opt>
    <opt isPrefix="1">-ax</opt>
    <opt isPrefix="true">-auto</opt>
    <opt isPrefix="true">-B</opt>
    <opt>-Bdynamic</opt>
    <opt>-Bstatic</opt>
    <opt>-big-endian</opt>
    <opt>-C</opt>
    <opt isIgnored="true" removePhases="build_config">-c</opt>
    <opt isPrefix="true" isIgnored="true">-check</opt>
    <opt isPrefix="true" isIgnored="true">-no-check</opt>
    <opt>-complex-limited-range</opt>
    <opt>-no-complex-limited-range</opt>
    <opt isPrefix="true">-cxxlib</opt>
    <opt>-no-cxxlib</opt>
    <opt isPrefix="true">-d</opt>
    <opt numArgs="1" isIgnored="true">-debug</opt>
    <opt isPrefix="true" isIgnored="true">-diag</opt>
    <opt isPrefix="true" isIgnored="true">-no-diag</opt>
    <opt isPrefix="true">-dynamic-linker</opt>
    <opt isPrefix="true" isIgnored="true">-dump</opt>
    <opt isIgnored="true">-dryrun</opt>
    <opt isIgnored="true">-E</opt>
    <opt isIgnored="true">-EP</opt>
    <opt>-no-restrict</opt>
    <opt>-early-template-check</opt>
    <opt>-no-early-template-check</opt>
    <opt isIgnored="true">-extern-prefix</opt>
    <opt isPrefix="true">-f</opt>
    <opt>-fast-transcendentals</opt>
    <opt>-no-fast-transcendentals</opt>
    <opt>-ffinite-math-only</opt>
    <opt>-fgnu89-inline</opt>
    <opt isPrefix="true">-fimf</opt>
    <opt>-fma</opt>
    <opt>-no-fma</opt>
    <opt numArgs="1">-fp-model</opt>
    <opt>-fp-speculation</opt>
    <opt>-fp-port</opt>
    <opt>-no-fp-port</opt>
    <opt>-fp-stack-check</opt>
    <opt isPrefix="true">-fp-trap</opt>
    <opt>-ftz</opt>
    <opt>-no-ftz</opt>
    <opt isPrefix="true">-g</opt>
    <opt>-gcc</opt>
    <opt>-no-gcc</opt>
    <opt isPrefix="true">-gcc-name</opt>
    <opt isPrefix="true">-gxx-name</opt>
    <opt>-gcc-sys</opt>
    <opt isIgnored="true">-global-hoist</opt>
    <opt isIgnored="true">-no-global-hoist</opt>
    <opt isPrefix="true">-gnu-prefix</opt>
    <opt isPrefix="true" isIgnored="true">-guide</opt>
    <opt isIgnored="true">-H</opt>
    <opt isPrefix="true" isIgnored="true">--help</opt>
    <opt numArgs="1">-idirafter</opt>
    <opt>-intel-extensions</opt>
    <opt>-no-intel-extensions</opt>
    <opt numArgs="1">-imacros</opt>
    <opt isPrefix="true">-inline</opt>
    <opt isPrefix="true">-no-inline</opt>
    <opt isPrefix="true" isIgnored="true">-ip</opt>
    <opt isIgnored="true">-no-ip</opt>
    <opt numArgs="1">-iprefix</opt>
    <opt numArgs="1">-iquote</opt>
    <opt numArgs="1">-iquote</opt>
    <opt numArgs="1">-isysroot</opt>
    <opt numArgs="1">-iwithprefix</opt>
    <opt numArgs="1">-iwithprefixbefore</opt>
    <opt>-Kc++</opt> <!-- deprecated but available -->
    <opt isPrefix="true">-L</opt>
    <opt isPrefix="true">-l</opt>
    <opt>-little-endian</opt>
    <opt isPrefix="true">-m</opt>
    <opt isIgnored="true">-map-opts</opt>
    <opt isPrefix="true" isIgnored="true">-mcmodel</opt>
    <opt isIgnored="true">-M</opt>
    <opt isIgnored="true">-MM</opt>
    <opt numArgs="1" isIgnored="true" removePhases="instrum">-MF</opt>
    <opt isIgnored="true">-MG</opt>
    <opt isIgnored="true" removePhases="instrum">-MP</opt>
    <opt numArgs="1" isIgnored="true" removePhases="instrum">-MT</opt>
    <opt numArgs="1" isIgnored="true" removePhases="instrum">-MQ</opt>
    <opt isIgnored="true" removePhases="instrum">-MD</opt>
    <opt isIgnored="true" removePhases="instrum">-MMD</opt>
    <opt>-multibyte-chars</opt>
    <opt>-no-multibyte-chars</opt>
    <opt>-multiline-strings</opt>
    <opt>-no-multiline-strings</opt>
    <opt numArgs="1" isIgnored="true">-multiple-processes</opt>
    <opt>-nodefaultlibs</opt>
    <opt>-nostartfiles</opt>
    <opt>-nostdlib</opt>
    <opt>-no-gcc-include-dir</opt>
    <opt>-no-icc</opt>
    <opt>-nostdinc</opt>
    <opt>-nostdinc++</opt>
    <opt isPrefix="true">-O</opt>
    <opt numArgs="1" isIgnored="true">-o</opt>
    <opt isIgnored="true">-P</opt>
    <opt isIgnored="true">-p</opt> <!-- Ignored along with -prof-* options -->
    <opt isPrefix="true" isIgnored="true">-par</opt>
    <opt isPrefix="true">-pc</opt>
    <opt isPrefix="true" isIgnored="true">-pch</opt>
    <opt isPrefix="true" isIgnored="true">-pe-</opt>
    <opt isIgnored="true">-no-pe-file-primary</opt>
    <opt isIgnored="true" removePhases="instrum">-pedantic</opt>
    <opt>-pie</opt>
    <opt>-no_pie</opt>
    <opt isPrefix="true">-pragma</opt>
    <opt isPrefix="true">-prec-div</opt>
    <opt isPrefix="true">-no-prec-div</opt>
    <opt isPrefix="true">-prec-sqrt</opt>
    <opt isPrefix="true">-no-prec-sqrt</opt>
    <opt isIgnored="true">-print-multi-lib</opt>
    <opt isPrefix="true" isIgnored="true">-prof</opt>
    <opt isPrefix="true" isIgnored="true">-no-prof</opt>
    <opt>-pthread</opt>
    <opt>-pg</opt>
    <opt isPrefix="true">-print</opt>
    <opt>-promote-varargs-to-64bit</opt>
    <opt>-pthread</opt>
    <opt isPrefix="true" isIgnored="true">-Q</opt>
    <opt isPrefix="true" isIgnored="true">-q</opt>
    <opt>-rcd</opt>
    <opt>-restrict</opt>
    <opt>-no-restrict</opt>
    <opt>-resolve-byte-order-mismatch</opt>
    <opt>-no-resolve-byte-order-mismatch</opt>
    <opt>-S</opt>
    <opt>-s</opt>
    <opt isIgnored="true">-save-temps</opt>
    <opt isIgnored="true">-scalar-rep</opt>
    <opt isIgnored="true">-no-scalar-rep</opt>
    <opt>-shared</opt>
    <opt>-shared-intel</opt>
    <opt>-shared-libgcc</opt>
    <opt>-simd</opt>
    <opt>-no-simd</opt>
    <opt>-simd-function-pointers</opt>
    <opt>-no-simd-function-pointers</opt>
    <opt>-size-llp64</opt>
    <opt isPrefix="true" isIgnored="true">-sox</opt>
    <opt>-standalone</opt>
    <opt>-static</opt>
    <opt>-static-intel</opt>
    <opt>-static-libgcc</opt>
    <opt>-static-libstdc++</opt>
    <opt>-save-temps</opt>
    <opt>-shared</opt>
    <opt>-static</opt>
    <opt numArgs="1">-std</opt>
    <opt>-strict-ansi</opt>
    <opt>-swap-pointers</opt>
    <opt>-no-swap-pointers</opt>
    <opt>-symcheck</opt>
    <opt numArgs="1" isIgnored="true">-T</opt>
    <opt isPrefix="true" isIgnored="true">-t</opt>
    <opt isPrefix="true" isIgnored="true">-tc</opt>
    <opt numArgs="1" isIgnored="true">-tcollect-filter</opt>
    <opt isPrefix="true" isIgnored="true">-traceback</opt>
    <opt isPrefix="true" isIgnored="true">-notraceback</opt>
    <opt>-trigraphs</opt>
    <opt isPrefix="true">-u</opt>
    <opt>-undef</opt>
    <opt isPrefix="true" isIgnored="true">-unroll</opt>
    <opt>-use-decl-byte-order</opt>
    <opt>-use-intel-optimized-headers</opt>
    <opt>-no-use-intel-optimized-headers</opt>
    <opt isIgnored="true">-V</opt>
    <opt>-v</opt>
    <opt>-vec</opt>
    <opt>-no-vec</opt>
    <opt isPrefix="true">-vec</opt>
    <opt>-no-vec-guard-write</opt>
    <opt isIgnored="true">--version</opt>
    <opt isPrefix="true" isIgnored="true" >-W</opt>
    <opt isIgnored="true" removePhases="instrum" >-Wall</opt>
    <opt isPrefix="true" isIgnored="true" removePhases="instrum" >-Werror</opt>
    <opt isPrefix="true">-Wp</opt>
    <opt isPrefix="true" isIgnored="true">-w</opt>
    <opt isIgnored="true">-nowatch</opt>
    <opt isIgnored="true">-X</opt>
    <opt numArgs="1">-Xassembler</opt>
    <opt numArgs="1">-Xlinker</opt>
    <opt numArgs="1">-Xpreprocessor</opt>
    <opt isPrefix="true">-q</opt>
    <opt isPrefix="1">-x</opt>
    <opt numArgs="1">-x</opt>
    <opt isPrefix="true">-Zp</opt>
  </semantic_options>
  <semantic_environment_variable_list>
    <env>COMPILER_PATH</env>
    <env>CPATH</env>
    <env>CPLUS_INCLUDE_PATH</env>
    <env>C_INCLUDE_PATH</env>
    <env>DEPENDENCIES_OUTPUT</env>
    <env>LANG</env>
    <env>LC_ALL</env>
    <env>LC_CTYPE</env>
    <env>LC_MESSAGES</env>
    <env>LIBRARY_PATH</env>
    <env>OBJC_INCLUDE_PATH</env>
    <env>SUNPRO_DEPENDENCIES</env>
  </semantic_environment_variable_list>
  <preprocess_options_list>
    <opt>-E</opt>
  </preprocess_options_list>
  <preprocess_dollar_macros>yes</preprocess_dollar_macros>
  <forbidden_macros_list>
    <name>__DEC128_EPSILON__</name>
    <name>__DEC128_MAX__</name>
    <name>__DEC128_MIN__</name>
    <name>__DEC128_SUBNORMAL_MIN__</name>
    <name>__DEC32_EPSILON__</name>
    <name>__DEC32_MAX__</name>
    <name>__DEC32_MIN__</name>
    <name>__DEC32_SUBNORMAL_MIN__</name>
    <name>__DEC64_EPSILON__</name>
    <name>__DEC64_MAX__</name>
    <name>__DEC64_MIN__</name>
    <name>__DEC64_SUBNORMAL_MIN__</name>
    <name>__DECIMAL_BID_FORMAT__</name>
    <name>__DEC_EVAL_METHOD__</name>
  </forbidden_macros_list>
  <compile_options_list>
    <opt>-c</opt>
  </compile_options_list>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>o</ext>
  </obj_extensions>
  <precompiled_header_extensions>
    <ext>gch</ext>
  </precompiled_header_extensions>
  <include_strategy>sniff</include_strategy>
  <from_compiler_macro_strategy>
    <input>
      <opt>-dM</opt>
      <opt>-E</opt>
      <empty_source_file>true</empty_source_file>
    </input>
    <output>
      <regex name_group="1" value_group="5">^#\s*define\s+(([^\s]*(\(.*\)))|([^\s]*))\s+(.*)?$</regex>
    </output>
  </from_compiler_macro_strategy>
</compiler_configuration>
