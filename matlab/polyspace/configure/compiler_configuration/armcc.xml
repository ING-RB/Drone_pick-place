<compiler_configuration>
  <compiler_names>
    <name>armcc</name>
  </compiler_names>
  <compiler_xml>
    <name>armcc</name>
    <opt numArgs="1">_noopt</opt>
    <target pattern="_" value="arm"/>
  </compiler_xml>
  <include_options>
    <opt>-I</opt>
  </include_options>
  <system_include_options>
    <opt>-J</opt>
    <opt>-isystem</opt>
  </system_include_options>
  <preinclude_options>
    <opt isPrefix="true">--preinclude</opt>
  </preinclude_options>
  <define_options>
    <opt isPrefix="true">-D</opt>
  </define_options>
  <undefine_options>
    <opt isPrefix="true">-U</opt>
  </undefine_options>
  <options_file_options>
    <opt>--via</opt>
    <opt>--Via</opt>
  </options_file_options>
  <output_options>
    <opt>-o</opt>
    <opt>--output_dir</opt>
  </output_options>
  <standard_options>
    <!--Default is -std=gnu11-->
    <std version="c90">-std=c90</std>
    <std version="c99">-std=c99</std>
    <std version="c11">-std=c11</std>
    <std version="c17">-std=c17</std>
    <!--Default is -std=gnu++14-->
    <std version="c++03">-std=c++03</std>
    <std version="c++11">-std=c++11</std>
    <std version="c++14">-std=c++14</std>
    <std version="c++17">-std=c++17</std>
    <std version="c++20">-std=c++20</std>
  </standard_options>
  <semantic_options>
    <opt isPrefix="true" isIgnored="true">-A</opt>
    <opt isIgnored="true">--allow_fpreg_for_nonfpdata</opt>
    <opt isIgnored="true">--no_allow_fpreg_for_nonfpdata</opt>
    <opt>--allow_null_this</opt>
    <opt>--no_allow_null_this</opt>
    <opt>--alternative_tokens</opt>
    <opt>--no_alternative_tokens</opt>
    <opt>--anachronisms</opt>
    <opt>--no_anachronisms</opt>
    <opt numArgs="1" isIgnored="true">--apcs</opt>
    <opt>--arm</opt>
    <opt>--arm_only</opt>
    <opt isIgnored="true">--asm</opt>
    <opt numArgs="1" isIgnored="true">--asm_dir</opt>
    <opt>--autoinline</opt>
    <opt>--no_autoinline</opt>
    <opt>--bigend</opt>
    <opt>--bitband</opt>
    <opt>--branch_tables</opt>
    <opt>--no_branch_tables</opt>
    <opt isIgnored="true">--brief_diagnostics</opt>
    <opt isIgnored="true">--no_brief_diagnostics</opt>
    <opt numArgs="1" isIgnored="true">--bss_threshold</opt>
    <opt isIgnored="true" removePhases="build_config">-c</opt>
    <opt isIgnored="true">-C</opt>
    <opt>--c90</opt>
    <opt>--C90</opt>
    <opt>--c99</opt>
    <opt>--C99</opt>
    <opt isIgnored="true">--code_gen</opt>
    <opt isIgnored="true">--no_code_gen</opt>
    <opt numArgs="1" isIgnored="true">--compatible</opt>
    <opt isIgnored="true">--compile_all_input</opt>
    <opt isIgnored="true">--no_compile_all_input</opt>
    <opt isIgnored="true">--conditionalize</opt>
    <opt isIgnored="true">--no_conditionalize</opt>
    <opt>--cpp</opt>
    <opt>--Cpp</opt>
    <opt>--CPP</opt>
    <opt>--cpp11</opt>
    <opt>--Cpp11</opt>
    <opt>--CPP11</opt>
    <opt>--cpp_compat</opt>
    <opt numArgs="1">--cpu</opt>
    <opt numArgs="1" isIgnored="true">--create_pch</opt>
    <opt numArgs="1" isIgnored="true">-D</opt>
    <opt isIgnored="true">--data_reorder</opt>
    <opt isIgnored="true">--no_data_reorder</opt>
    <opt isIgnored="true">--debug</opt>
    <opt isIgnored="true">--no_debug</opt>
    <opt isIgnored="true">--debug_macros</opt>
    <opt isIgnored="true">--no_debug_macros</opt>
    <opt numArgs="1" isIgnored="true">--default_extension</opt>
    <opt>--dep_name</opt>
    <opt>--no_dep_name</opt>
    <opt numArgs="1" isIgnored="true">--depend</opt>
    <opt numArgs="1" isIgnored="true">--depend_dir</opt>
    <opt numArgs="1" isIgnored="true">--depend_format</opt>
    <opt isIgnored="true">--depend_single_line</opt>
    <opt isIgnored="true">--no_depend_single_line</opt>
    <opt isIgnored="true">--depend_system_headers</opt>
    <opt isIgnored="true">--no_depend_system_headers</opt>
    <opt numArgs="1" isIgnored="true">--depend_target</opt>
    <opt numArgs="1" isIgnored="true">--diag_error</opt>
    <opt numArgs="1" isIgnored="true">--diag_remark</opt>
    <opt numArgs="1" isIgnored="true">--diag_style</opt>
    <opt numArgs="1" isIgnored="true">--diag_suppress</opt>
    <opt numArgs="1" isIgnored="true">--diag_warning</opt>
    <opt>--dollar</opt>
    <opt>--no_dollar</opt>
    <opt isPrefix="true" isIgnored="true">--dwarf</opt>
    <opt isIgnored="true">--echo</opt>
    <opt isIgnored="true">--emit_frame_directives</opt>
    <opt isIgnored="true">--no_emit_frame_directives</opt>
    <opt>--enum_is_int</opt>
    <opt numArgs="1" isIgnored="true">--errors</opt>
    <opt>--exceptions</opt>
    <opt>--no_exceptions</opt>
    <opt>--exceptions_unwind</opt>
    <opt>--no_exceptions_unwind</opt>
    <opt isIgnored="true">--execute_only</opt>
    <opt>--extended_initializers</opt>
    <opt>--no_extended_initializers</opt>
    <opt numArgs="1" isIgnored="true">--feedback</opt>
    <opt isIgnored="true">--float_literal_pools</opt>
    <opt isIgnored="true">--no_float_literal_pools</opt>
    <opt>--force_new_nothrow</opt>
    <opt>--no_force_new_nothrow</opt>
    <opt>--forceinline</opt>
    <opt numArgs="1">--fp16_format</opt>
    <opt numArgs="1">--fpmode</opt>
    <opt numArgs="1">--fpu</opt>
    <opt>--friend_injection</opt>
    <opt>--no_friend_injection</opt>
    <opt isIgnored="true">-g</opt>
    <opt numArgs="1" isIgnored="true">--global_reg</opt>
    <opt>--guiding_decls</opt>
    <opt>--no_guiding_decls</opt>
    <opt isIgnored="true">--help</opt>
    <opt isIgnored="true">--ignore_missing_headers</opt>
    <opt>--implicit_include</opt>
    <opt>--no_implicit_include</opt>
    <opt>--implicit_include_searches</opt>
    <opt>--no_implicit_include_searches</opt>
    <opt>--implicit_key_function</opt>
    <opt>--no_implicit_key_function</opt>
    <opt>--implicit_typename</opt>
    <opt>--no_implicit_typename</opt>
    <opt numArgs="1" isIgnored="true">--info</opt>
    <opt>--inline</opt>
    <opt>--no_inline</opt>
    <opt isIgnored="true">--integer_literal_pools</opt>
    <opt isIgnored="true">--no_integer_literal_pools</opt>
    <opt>--interface_enums_are_32_bit</opt>
    <opt isIgnored="true">--interleave</opt>
    <opt>--kandr_include</opt>
    <opt isPrefix="true" isIgnored="true">-L</opt>
    <opt numArgs="1" isIgnored="true">--library_interface</opt>
    <opt numArgs="1" isIgnored="true">--library_type</opt>
    <opt numArgs="1" isIgnored="true">--liclinger</opt>
    <opt isIgnored="true">--licretry</opt>
    <opt isIgnored="true">--link_all_input</opt>
    <opt isIgnored="true">--no_link_all_input</opt>
    <opt isIgnored="true">--list</opt>
    <opt numArgs="1" isIgnored="true">--list_dir</opt>
    <opt isIgnored="true">--list_macros</opt>
    <opt>--littleend</opt>
    <opt numArgs="1">--locale</opt>
    <opt>--long_long</opt>
    <opt numArgs="1" isIgnored="true">--loop_optimization_level</opt>
    <opt>--loose_implicit_cast</opt>
    <opt>--lower_ropi</opt>
    <opt>--no_lower_ropi</opt>
    <opt>--lower_rwpi</opt>
    <opt>--no_lower_rwpi</opt>
    <opt isIgnored="true">-M</opt>
    <opt isIgnored="true">--md</opt>
    <opt numArgs="1" isIgnored="true">--message_locale</opt>
    <opt numArgs="1">--min_array_alignment</opt>
    <opt isIgnored="true">--mm</opt>
    <opt>--multibyte_chars</opt>
    <opt>--no_multibyte_chars</opt>
    <opt isIgnored="true">--multifile</opt>
    <opt isIgnored="true">--no_multifile</opt>
    <opt numArgs="1" isIgnored="true">--multiply_latency</opt>
    <opt>--narrow_volatile_bitfields</opt>
    <opt>--nonstd_qualifier_deduction</opt>
    <opt>--no_nonstd_qualifier_deduction</opt>
    <opt isPrefix="true" isIgnored="true">-O</opt>
    <opt>--old_specializations</opt>
    <opt>--no_old_specializations</opt>
    <opt>--old_style_preprocessing</opt>
    <opt numArgs="1" isIgnored="true">--omf_browse</opt>
    <opt isIgnored="true">--ool_section_name</opt>
    <opt isIgnored="true">--no_ool_section_name</opt>
    <opt isIgnored="true">-P</opt>
    <opt>--parse_templates</opt>
    <opt>--no_parse_templates</opt>
    <opt isIgnored="true">--pch</opt>
    <opt numArgs="1" isIgnored="true">--pch_dir</opt>
    <opt isIgnored="true">--pch_messages</opt>
    <opt isIgnored="true">--no_pch_messages</opt>
    <opt isIgnored="true">--pch_verbose</opt>
    <opt isIgnored="true">--no_pch_verbose</opt>
    <opt numArgs="1">--pending_instantiations</opt>
    <opt isIgnored="true">--phony_targets</opt>
    <opt numArgs="1">--pointer_alignment</opt>
    <opt isIgnored="true">--preprocess_assembly</opt>
    <opt isIgnored="true">--preprocessed</opt>
    <opt isIgnored="true">--protect_stack</opt>
    <opt isIgnored="true">--no_protect_stack</opt>
    <opt isIgnored="true">--reassociate_saturation</opt>
    <opt isIgnored="true">--no_reassociate_saturation</opt>
    <opt>--reduce_paths</opt>
    <opt>--no_reduce_paths</opt>
    <opt isIgnored="true">--remarks</opt>
    <opt isIgnored="true">--remove_unneeded_entities</opt>
    <opt isIgnored="true">--no_remove_unneeded_entities</opt>
    <opt>--restrict</opt>
    <opt>--no_restrict</opt>
    <opt numArgs="1" isIgnored="true">--retain</opt>
    <opt>--rtti</opt>
    <opt>--no_rtti</opt>
    <opt>--rtti_data</opt>
    <opt>--no_rtti_data</opt>
    <opt isIgnored="true">-S</opt>
    <opt>--share_inlineable_strings</opt>
    <opt>--no_share_inlineable_strings</opt>
    <opt isIgnored="true">--show_cmdline</opt>
    <opt>--signed_bitfields</opt>
    <opt>--unsigned_bitfields</opt>
    <opt>--signed_chars</opt>
    <opt>--unsigned_chars</opt>
    <opt isIgnored="true">--split_ldm</opt>
    <opt isIgnored="true">--split_sections</opt>
    <opt>--strict</opt>
    <opt>--no_strict</opt>
    <opt isIgnored="true">--strict_warnings</opt>
    <opt isIgnored="true">--string_literal_pools</opt>
    <opt isIgnored="true">--no_string_literal_pools</opt>
    <opt>--sys_include</opt>
    <opt isIgnored="true">--thumb</opt>
    <opt>--trigraphs</opt>
    <opt>--no_trigraphs</opt>
    <opt>--type_traits_helpers</opt>
    <opt>--no_type_traits_helpers</opt>
    <opt>--unaligned_access</opt>
    <opt>--no_unaligned_access</opt>
    <opt isIgnored="true">--use_frame_pointer</opt>
    <opt isIgnored="true">--no_use_frame_pointer</opt>
    <opt numArgs="1" isIgnored="true">--use_pch</opt>
    <opt>--using_std</opt>
    <opt>--no_using_std</opt>
    <opt isIgnored="true">--version_number</opt>
    <opt>--vfe</opt>
    <opt>--no_vfe</opt>
    <opt>--vla</opt>
    <opt>--no_vla</opt>
    <opt isIgnored="true">--vsn</opt>
    <opt isIgnored="true">-W</opt>
    <opt>--wchar</opt>
    <opt>--no_wchar</opt>
    <opt>--wchar16</opt>
    <opt>--wchar32</opt>
    <opt isIgnored="true">--whole_program</opt>
    <opt isIgnored="true">--wrap_diagnostics</opt>
    <opt isIgnored="true">--no_wrap_diagnostics</opt>
  </semantic_options>
  <preprocess_options_list>
    <opt>-E</opt>
  </preprocess_options_list>
  <preprocess_dollar_macros>yes</preprocess_dollar_macros>
  <forbidden_macros_list></forbidden_macros_list>
  <compile_options_list>
    <opt>-c</opt>
  </compile_options_list>
  <compiler>armcc</compiler>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>obj</ext>
    <ext>o</ext>
  </obj_extensions>
  <default_macro_strategy>from-compiler</default_macro_strategy>
  <from_compiler_macro_strategy>
    <input>
      <opt>--list_macros</opt>
      <empty_source_file>true</empty_source_file>
    </input>
    <output>
      <regex name_group="1" value_group="5">^#\s*define\s+(([^\s]*(\(.*\)))|([^\s]*))\s+(.*)?$</regex>
    </output>
  </from_compiler_macro_strategy>
  <polyspace_c_extra_options_list></polyspace_c_extra_options_list>
  <polyspace_cpp_extra_options_list></polyspace_cpp_extra_options_list>
</compiler_configuration>
