<!-- XC8 will be detected as a clang when tracing the compilation, this file is only used with the other workflows -->
<compiler_configuration>
  <compiler_names>
    <name>xc8-cc</name>
  </compiler_names>
  <compiler_xml>
    <name>microchip</name>
    <opt numArgs="1">_noopt</opt>
    <target pattern="_" value="pic"/>
  </compiler_xml>
  <compiler>microchip</compiler>
  <include_options>
    <opt>-I</opt>
  </include_options>
  <system_include_options>
    <opt>-isystem</opt>
  </system_include_options>
  <preinclude_options>
    <opt>-include</opt>
    <opt>--include</opt>
  </preinclude_options>
  <define_options>
    <opt>-D</opt>
  </define_options>
  <undefine_options>
    <opt>-U</opt>
  </undefine_options>
  <options_file_options>
    <opt>@</opt>
  </options_file_options>
  <output_options>
    <opt>-o</opt>
  </output_options>
  <standard_options>
    <std version="c90">-std=c90</std>
    <std version="c99" c_default="1">-std=c99</std>
  </standard_options>
  <semantic_options>
    <opt isIgnored="true">-###</opt>
    <opt isIgnored="true">-ansi</opt>
    <opt isIgnored="true" removePhases="build_config">-c</opt>
    <opt isIgnored="true">-C</opt>
    <opt numArgs="1">--config</opt>
    <opt numArgs="1">-dependency-file</opt>
    <opt isPrefix="true" isIgnored="true">-d</opt>
    <opt isIgnored="true">-E</opt>
    <opt isPrefix="true" isIgnored="true">-f</opt>
    <opt>-fcp</opt>
    <opt isPrefix="true" isIgnored="true">--fill</opt>
    <opt isPrefix="true">-f</opt>
    <opt isPrefix="true" isIgnored="true">-g</opt>
    <opt isIgnored="true">-H</opt>
    <opt isIgnored="true">-help</opt>
    <opt isIgnored="true">--help</opt>
    <opt isPrefix="true">--include-with-prefix</opt>
    <opt isPrefix="true">-imacros</opt>
    <opt isPrefix="true">--imacros</opt>
    <opt isPrefix="true">-isysroot</opt>
    <opt isPrefix="true">-iwithprefix</opt>
    <opt isPrefix="true">-iwithsysroot</opt>
    <opt isPrefix="true" isIgnored="true">-l</opt>
    <opt isPrefix="true" isIgnored="true">-L</opt>
    <opt isPrefix="true" isIgnored="true">-M</opt>
    <opt numArgs="1" isIgnored="true">-MF</opt>
    <opt numArgs="1" isIgnored="true">-MT</opt>
    <opt numArgs="1" isIgnored="true">-MQ</opt>
    <opt isPrefix="true">-m</opt>
    <opt isIgnored="true">--memorysummary</opt>
    <opt isPrefix="true" isIgnored="true">-no</opt>
    <opt>-nobuiltininc</opt>
    <opt>-nodefaultlibs</opt>
    <opt>--nofallback</opt>
    <opt>-nostartfiles</opt>
    <opt>--no-standard-includes</opt>
    <opt>-nostdinc</opt>
    <opt>-nostdinc++</opt>
    <opt>-nostdlib</opt>
    <opt>-nostdlib++</opt>
    <opt>-nostdlibinc</opt>
    <opt isPrefix="true">--no-system-header-prefix</opt>
    <opt isPrefix="true" isIgnored="true">--no</opt>
    <opt isPrefix="true" isIgnored="true">-o</opt>
    <opt isPrefix="true" isIgnored="true">-O</opt>
    <opt isPrefix="true" isIgnored="true">--output</opt>
    <opt isIgnored="true">-pedantic</opt>
    <opt isIgnored="true">-pedantic-errors</opt>
    <opt>-pipe</opt>
    <opt>--pipe</opt>
    <opt isPrefix="true">--prefix</opt>
    <opt isPrefix="true" isIgnored="true">-print</opt>
    <opt isPrefix="true" isIgnored="true">-Q</opt>
    <opt isPrefix="true" isIgnored="true">-R</opt>
    <opt isIgnored="true">-S</opt>
    <opt isPrefix="true" isIgnored="true">-save-temps</opt>
    <opt isPrefix="true">-std=</opt>
    <opt isPrefix="true">--std=</opt>
    <opt isPrefix="true">--system-header-prefix</opt>
    <opt isIgnored="true">--target-help</opt>
    <opt isIgnored="true">--trace-includes</opt>
    <opt isIgnored="true">-v</opt>
    <opt isIgnored="true">--version</opt>
    <opt isIgnored="true">-w</opt>
    <opt isPrefix="true" isIgnored="true">-W</opt>
    <opt numArgs="1">-x</opt>
    <opt isPrefix="true" isIgnored="true">-X</opt>
    <opt isPrefix="true">-Xclang</opt>
    <opt isPrefix="true">-Xpreprocessor</opt>
  </semantic_options>
  <preprocess_options_list>
    <opt>-E</opt>
  </preprocess_options_list>
  <preprocessed_output_file>$(SOURCE_FILE_NO_EXT).i</preprocessed_output_file>
  <preprocess_dollar_macros>yes</preprocess_dollar_macros>
  <forbidden_macros_list>
    <name>__has_builtin</name>
    <name>__has_feature</name>
    <name>__has_extension</name>
    <name>__has_attribute</name>
    <name>__has_warning</name>
  </forbidden_macros_list>
  <compile_options_list>
    <opt>-c</opt>
  </compile_options_list>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>obj</ext>
    <ext>o</ext>
  </obj_extensions>
  <include_strategy>sniff</include_strategy>
  <polyspace_extra_options_list></polyspace_extra_options_list>
</compiler_configuration>
