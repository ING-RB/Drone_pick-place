<compiler_configuration>
  <compiler_names>
    <name>ccrl</name>
  </compiler_names>
  <compiler_xml>
    <name>renesas</name>
    <opt numArgs="1">_noopt</opt>
    <target pattern="_" value="rl78"/>
  </compiler_xml>
  <compiler>renesas</compiler>
  <include_options>
    <opt>-I</opt>
  </include_options>
  <system_include_options/> <!-- N/A -->
  <preinclude_options>
    <opt>-preinclude</opt>
  </preinclude_options>
  <define_options>
    <opt>-D</opt>
  </define_options>
  <undefine_options>
    <opt>-U</opt>
  </undefine_options>
  <options_file_options>
    <opt>-subcommand</opt>
  </options_file_options>
  <output_options>
    <opt>-o</opt>
    <opt>--output</opt>
    <opt>-obj_path</opt>
  </output_options>
  <standard_options>
    <std version="c90" c_default="1">-lang=c</std>
    <std version="c99">-lang=c99</std>
  </standard_options>
  <semantic_options>
    <opt isIgnored="true">-ansi</opt>
    <opt isPrefix="true" isIgnored="true">-asm_path</opt>
    <opt isPrefix="true" isIgnored="true">-asmcmd</opt>
    <opt isPrefix="true" isIgnored="true">-asmopt</opt>
    <opt isIgnored="true" removePhases="build_config">-c</opt>
    <opt isPrefix="true" isIgnored="true">-character_set</opt>
    <opt isIgnored="true">-check_language_extension</opt>
    <opt isPrefix="true" isIgnored="true">-convert_cc</opt>
    <opt isPrefix="true">-cpu</opt>
    <opt isPrefix="true" isIgnored="true">-cref</opt>
    <opt isPrefix="true">-dbl_size</opt>
    <opt isPrefix="true" isIgnored="true">-dev</opt>
    <opt isPrefix="true" isIgnored="true">-error_file</opt>
    <opt>-far_rom</opt>
    <opt isIgnored="true">-g</opt>
    <opt isIgnored="true">-g_line</opt>
    <opt isIgnored="true">-goptimize</opt>
    <opt isIgnored="true">-help</opt>
    <opt isPrefix="true" isIgnored="true">-ignore_files_misra</opt>
    <opt isPrefix="true" isIgnored="true">-insert_nop_with_label</opt>
    <opt>-large_variable</opt>
    <opt isPrefix="true" isIgnored="true">-lnkcmd</opt>
    <opt isPrefix="true" isIgnored="true">-lnkopt</opt>
    <opt isPrefix="true">-memory_model</opt>
    <opt>-merge_string</opt>
    <opt isPrefix="true" isIgnored="true">-misra2004</opt>
    <opt isPrefix="true" isIgnored="true">-misra2012</opt>
    <opt>-nest_comment</opt>
    <opt isPrefix="true">-no_warning_num</opt>
    <opt isPrefix="true" isIgnored="true">-O</opt>
    <opt isIgnored="true">-P</opt>
    <opt>-pack</opt>
    <opt isIgnored="true">-pass_source</opt>
    <opt isPrefix="true" isIgnored="true">-prep_path</opt>
    <opt isPrefix="true" isIgnored="true">-preprocess</opt>
    <opt isIgnored="true">-refs_without_declaration</opt>
    <opt isIgnored="true">-S</opt>
    <opt>-signed_bitfield</opt>
    <opt>-signed_char</opt>
    <opt isPrefix="true" isIgnored="true">-stack_protector</opt>
    <opt isPrefix="true" isIgnored="true">-switch</opt>
    <opt isPrefix="true">-use_mda</opt>
    <opt isIgnored="true">-V</opt>
    <opt>-volatile</opt>
  </semantic_options>
  <preprocess_options_list>
    <opt>-P</opt>
    <opt>-o</opt>
    <opt>$(OUTPUT_FILE)</opt>
  </preprocess_options_list>
  <preprocess_dollar_macros>no</preprocess_dollar_macros>
  <strict_option_names>yes</strict_option_names>
  <forbidden_macros_list>
  </forbidden_macros_list>
  <compile_options_list>
    <opt>-c</opt>
  </compile_options_list>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>obj</ext>
  </obj_extensions>
  <precompiled_header_extensions>
  </precompiled_header_extensions>
</compiler_configuration>
