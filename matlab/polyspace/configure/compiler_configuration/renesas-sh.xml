<compiler_configuration>
  <compiler_names>
    <name>(shc|shcpp)</name>
  </compiler_names>
  <compiler_xml>
    <name>renesas</name>
    <opt numArgs="1">_noopt</opt>
    <target pattern="_" value="sh"/>
  </compiler_xml>
  <compiler>renesas</compiler>
  <case_sensitive>0</case_sensitive>
  <include_options>
    <opt isPrefix="true" separator=",">-include=</opt>
  </include_options>
  <system_include_options/> <!-- N/A -->
  <preinclude_options>
    <opt isPrefix="true" separator=",">-preinclude=</opt>
  </preinclude_options>
  <define_options>
    <opt isPrefix="true" separator=",">-define=</opt>
  </define_options>
  <undefine_options/> <!-- N/A -->
  <options_file_options>
    <opt>-subcommand</opt>
  </options_file_options>
  <output_options>
    <opt>-objectfile</opt>
  </output_options>
  <standard_options>
    <std version="c90" c_default="1">-lang=c</std>
    <std version="c99">-lang=c99</std>
    <std version="c++03" cpp_default="1"></std>
  </standard_options>
  <semantic_options>
    <opt isPrefix="true" isIgnored="true">-ab</opt>
    <opt isPrefix="true">-alias</opt>
    <opt isPrefix="true">-align</opt>
    <opt isPrefix="true" isIgnored="true">-ap</opt>
    <opt isPrefix="true">-au</opt>
    <opt isPrefix="true">-bi</opt>
    <opt isPrefix="true" isIgnored="true">-blo</opt>
    <opt isPrefix="true" isIgnored="true">-bs</opt>
    <opt isPrefix="true" isIgnored="true" removePhases="build_config">-c</opt>
    <opt isPrefix="true" isIgnored="true">-ca</opt>
    <opt isPrefix="true" isIgnored="true">-cha</opt>
    <opt isPrefix="true">-com</opt>
    <opt isPrefix="true" isIgnored="true">-const</opt>
    <opt isPrefix="true">-cp</opt>
    <opt isPrefix="true">-cpp_noinline</opt>
    <opt isPrefix="true" isIgnored="true">-deb</opt>
    <opt isPrefix="true">-del</opt>
    <opt isPrefix="true">-den</opt>
    <opt isPrefix="true" isIgnored="true">-di</opt>
    <opt isPrefix="true">-do</opt>
    <opt isPrefix="true">-ds</opt>
    <opt isPrefix="true">-e</opt> errorpath
    <opt isPrefix="true">-ec</opt>
    <opt isPrefix="true">-en</opt>
    <opt isPrefix="true">-ena</opt>
    <opt isPrefix="true">-ex</opt>
    <opt isPrefix="true" isIgnored="true">-fdi</opt>
    <opt isPrefix="true" isIgnored="true">-fil</opt>
    <opt isPrefix="true">-fixed</opt>
    <opt isPrefix="true" isIgnored="true">-float_order</opt>
    <opt isPrefix="true">-fp</opt>
    <opt isPrefix="true" isIgnored="true">-fps</opt>
    <opt isPrefix="true" isIgnored="true">-g</opt>
    <opt isPrefix="true" isIgnored="true">-gb</opt>
    <opt isPrefix="true">-global</opt>
    <opt isPrefix="true" isIgnored="true">-h</opt>
    <opt isPrefix="true" isIgnored="true">-ifu</opt>
    <opt isPrefix="true" isIgnored="true">-inf</opt>
    <opt isPrefix="true" isIgnored="true">-inl</opt>
    <opt isPrefix="true" isIgnored="true">-l</opt>
    <opt isPrefix="true">-la</opt>
    <opt isPrefix="true">-lat</opt>
    <opt isPrefix="true" isIgnored="true">-libmk</opt> <!-- Undocumented IDE option -->
    <opt isPrefix="true" isIgnored="true">-lo</opt>
    <opt isPrefix="true" isIgnored="true">-logi</opt>
    <opt isPrefix="true" isIgnored="true">-logo</opt>
    <opt isPrefix="true" isIgnored="true">-m</opt>
    <opt isPrefix="true" isIgnored="true">-map</opt>
    <opt isPrefix="true" isIgnored="true">-max</opt>
    <opt isPrefix="true" isIgnored="true">-me</opt>
    <opt isPrefix="true">-noal</opt>
    <opt isPrefix="true" isIgnored="true">-nodeb</opt>
    <opt isPrefix="true">-noex</opt>
    <opt isPrefix="true" isIgnored="true">-noinl</opt>
    <opt isPrefix="true" isIgnored="true">-nol</opt>
    <opt isPrefix="true" isIgnored="true">-nolin</opt>
    <opt isPrefix="true" isIgnored="true">-nolo</opt>
    <opt isPrefix="true" isIgnored="true">-nologo</opt>
    <opt isPrefix="true" isIgnored="true">-nome</opt>
    <opt isPrefix="true" isIgnored="true">-nort</opt>
    <opt isPrefix="true" isIgnored="true">-nosc</opt>
    <opt isPrefix="true" isIgnored="true">-nosp</opt>
    <opt isPrefix="true" isIgnored="true">-nost</opt>
    <opt isPrefix="true" isIgnored="true">-nouse_div_inst</opt>
    <opt isPrefix="true" isIgnored="true">-ob</opt>
    <opt isPrefix="true" isIgnored="true">-op</opt>
    <opt isPrefix="true" isIgnored="true">-ou</opt>
    <opt isPrefix="true" isIgnored="true">-p</opt>
    <opt isPrefix="true" isIgnored="true">-pa</opt>
    <opt isPrefix="true">-pack</opt>
    <opt isPrefix="true" isIgnored="true">-prep</opt>
    <opt isPrefix="true" isIgnored="true">-r</opt>
    <opt isPrefix="true" isIgnored="true">-rep</opt>
    <opt isPrefix="true" isIgnored="true">-rt</opt>
    <opt isPrefix="true">-rtti</opt>
    <opt isPrefix="true" isIgnored="true">-sa</opt>
    <opt isPrefix="true" isIgnored="true">-sc</opt>
    <opt isPrefix="true" isIgnored="true">-sco</opt>
    <opt isPrefix="true" isIgnored="true">-se</opt>
    <opt isPrefix="true" isIgnored="true">-sh</opt>
    <opt isPrefix="true" isIgnored="true">-shi</opt>
    <opt isPrefix="true" isIgnored="true">-si</opt>
    <opt isPrefix="true" isIgnored="true">-sim</opt>
    <opt isPrefix="true">-sj</opt>
    <opt isPrefix="true" isIgnored="true">-sm</opt>
    <opt isPrefix="true" isIgnored="true">-so</opt>
    <opt isPrefix="true" isIgnored="true">-sp</opt>
    <opt isPrefix="true" isIgnored="true">-st</opt>
    <opt isPrefix="true">-stri</opt>
    <opt isPrefix="true" isIgnored="true">-struct_a</opt>
    <opt isPrefix="true" isIgnored="true">-stu</opt>
    <opt isPrefix="true">-t</opt>
    <opt isPrefix="true" isIgnored="true">-tbr</opt>
    <opt isPrefix="true" isIgnored="true">-u</opt>
    <opt isPrefix="true" isIgnored="true">-v</opt>
  </semantic_options>
  <preprocess_options_list>
    <opt>-prep=$(OUTPUT_FILE)</opt>
  </preprocess_options_list>
  <preprocess_dollar_macros>no</preprocess_dollar_macros>
  <strict_option_names>yes</strict_option_names>
  <forbidden_macros_list>
  </forbidden_macros_list>
  <compile_options_list>
  </compile_options_list>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>obj</ext>
  </obj_extensions>
  <precompiled_header_extensions>
  </precompiled_header_extensions>
</compiler_configuration>
