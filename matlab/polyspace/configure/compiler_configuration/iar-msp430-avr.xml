<compiler_configuration>
  <compiler_names>
    <name>icc(avr|430)</name>
  </compiler_names>
  <compiler>iar-ew</compiler>
  <include_options>
    <opt>-I</opt>
  </include_options>
  <system_include_options>
    <opt>--system_include_dir</opt>
  </system_include_options>
  <preinclude_options>
    <opt>--preinclude</opt>
  </preinclude_options>
  <define_options>
    <opt>-D</opt>
  </define_options>
  <undefine_options>
    <opt>-U</opt>
  </undefine_options>
  <options_file_options>
    <opt>-f</opt>
  </options_file_options>
  <standard_options>
    <std version="c90">--c89</std>
    <std version="c99" c_default="1"></std>
    <std version="c++03" cpp_default="1"></std>
  </standard_options>
  <semantic_options>
    <opt>-2</opt>  <!-- 64-bit floating point for M32C targets -->
    <opt>--64bit_doubles</opt>
    <opt numArgs="1">--accumulators</opt>
    <opt>--arm</opt>
    <opt numArgs="1">--avr32_dsp_instructions</opt>
    <opt numArgs="1">--avr32_flashvault</opt>
    <opt numArgs="1">--avr32_fpu_instructions</opt>
    <opt numArgs="1">--avr32_rmw_instructions</opt>
    <opt numArgs="1">--avr32_simd_instructions</opt>
    <opt numArgs="1">--bus_width</opt>
    <opt>--c++</opt>
    <opt>--c89</opt>
    <opt numArgs="1">--calling_convention</opt>
    <opt>--char_is_signed</opt>
    <opt>--char_is_unsigned</opt>
    <opt>--clib</opt>
    <opt numArgs="1">--code_model</opt>
    <opt numArgs="1">--core</opt>
    <opt numArgs="1">--core_revision</opt>
    <opt numArgs="1">--cpu</opt>
    <opt numArgs="1">--cpu_info</opt>
    <opt numArgs="1">--data_model</opt>
    <opt>--debug</opt>
    <opt>--debug_lib</opt>
    <opt numArgs="1" isIgnored="true">--diag_suppress</opt>
    <opt>--direct_library_calls</opt>
    <opt>--dlib</opt>
    <opt numArgs="1">--dlib_config</opt>
    <opt numArgs="1">--double</opt>
    <opt>--dptr</opt>
    <opt>-e</opt>
    <opt>--ec++</opt>
    <opt>--eec++</opt>
    <opt numArgs="1" isIgnored="true">--eeprom_size</opt>
    <opt>--enable_mac</opt>
    <opt>--enable_multibytes</opt>
    <opt>--enable_restrict</opt>
    <opt numArgs="1">--endian</opt>
    <opt>--enum_is_int</opt>
    <opt>--generate_callt_runtime_library_calls</opt>
    <opt>--generate_far_runtime_library_calls</opt>
    <opt>--initializers_in_flash</opt>
    <opt numArgs="1">--int</opt>
    <opt numArgs="1">--interrupt_mode</opt>
    <opt numArgs="1">--isa</opt>
    <opt>--library_module</opt>
    <opt numArgs="1">--lock_regs</opt>
    <opt isUnsupported="1">--migration_preprocessor_extensions</opt>
    <opt isPrefix="true">-m</opt>
    <opt isIgnored="true">--misrac_verbose</opt>
    <opt isIgnored="true">--misrac1998</opt>
    <opt isIgnored="true">--misrac2004</opt>
    <opt numArgs="1">--memory_model</opt>
    <opt numArgs="1">--near_const_location</opt>
    <opt>--no_clustering</opt>
    <opt>--no_code_motion</opt>
    <opt>--no_cross_call</opt>
    <opt>--no_cse</opt>
    <opt>--no_exceptions</opt>
    <opt>--no_inline</opt>
    <opt>--no_path_in_file_macros</opt>
    <opt>--no_rtti</opt>
    <opt>--no_scheduling</opt>
    <opt>--no_static_destruction</opt>
    <opt>--no_system_include</opt>
    <opt>--no_tbaa</opt>
    <opt>--no_unroll</opt>
    <opt>--no_typedefs_in_diagnostics</opt>
    <opt>--warnings</opt>
    <opt isPrefix="true">-O</opt>
    <opt numArgs="1" isIgnored="true">-o</opt>
    <opt>--omit_types</opt>
    <opt numArgs="1">--operating_mode</opt>
    <opt numArgs="1">--pending_instantiations</opt>
    <opt>--place_const_in_code</opt>
    <opt>--require_prototypes</opt>
    <opt>-r</opt>
    <opt>--ropi</opt>
    <opt numArgs="1">--stack_pointer_size</opt>
    <opt>--strict</opt>
    <opt>--strict_ansi</opt>
    <opt>--use_c++_inline</opt>
    <opt isPrefix="1">-v</opt>
    <opt>--vectorize</opt>
    <opt>--vla</opt>
  </semantic_options>
  <switch>
    <case>
      <condition>
        <macro_defined name="__ICCAVR__"/>
      </condition>
      <actions>
        <target_name>avr</target_name>
      </actions>
    </case>
    <case>
      <condition>
        <macro_defined name="__ICC430__"/>
      </condition>
      <actions>
        <target_name>msp430</target_name>
      </actions>
    </case>
  </switch>
  <preprocess_options_list>
    <opt>--preprocess=n</opt>
    <opt>-</opt>
  </preprocess_options_list>
  <preprocess_dollar_macros>no</preprocess_dollar_macros>
  <strict_option_names>yes</strict_option_names>
  <forbidden_macros_list>
  </forbidden_macros_list>
  <compile_options_list>
  </compile_options_list>
  <src_extensions>
    <ext>c</ext>
    <ext>cc</ext>
    <ext>cp</ext>
    <ext>cxx</ext>
    <ext>cpp</ext>
    <ext>c++</ext>
  </src_extensions>
  <obj_extensions>
    <ext>o</ext>
  </obj_extensions>
  <precompiled_header_extensions>
  </precompiled_header_extensions>
  <include_strategy>sniff</include_strategy>
</compiler_configuration>
