<linker_configuration>
    <linker_names>
        <name>(.*-)?ld(-.+)?</name>
        <name>(.*-)?(ld\.)?gold(-.+)?</name>
        <name>(.*-)?ar(-.+)?</name>
    </linker_names>
    <library_options>
        <opt>-l</opt>
        <opt>-l:</opt>
        <opt>--library</opt>
    </library_options>
    <library_path_options>
        <opt>-L</opt>
        <opt>--library-paths</opt>
    </library_path_options>
    <ignored_options>
    </ignored_options>
    <obj_extensions>
        <ext>o</ext>
    </obj_extensions>
</linker_configuration>